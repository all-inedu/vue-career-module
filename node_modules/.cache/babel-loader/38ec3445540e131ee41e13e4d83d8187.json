n\n<style scoped>\n.navbar-brand {\n  width: 14%;\n  margin-left: -20px;\n}\n\n.btn-login {\n  background: #243876;\n  border-radius: 30px;\n  padding: 5px 20px !important;\n  cursor: pointer;\n  color: #fff !important;\n  transition: all 0.4s ease;\n}\n\n.btn-login:hover {\n  background: #eeaa56;\n}\n\n@media screen and (max-width: 600px) {\n  .navbar-brand {\n    width: 40%;\n    margin-left: -20px;\n  }\n}\n</style>\n\n<script>\nimport Auth from \"@/components/Home/Auth/Auth\";\n// @ is an alias to /src\n\nexport default {\n  name: \"Navbar\",\n  components: {\n    Auth,\n  },\n  data() {\n    return {\n      showLogin: false,\n      user: [],\n    };\n  },\n  methods: {\n    checkUser() {\n      if (sessionStorage.getItem(\"user\") != null) {\n        this.showLogin = false;\n        this.user = JSON.parse(sessionStorage.getItem(\"user\"));\n      } else {\n        this.user = sessionStorage.getItem(\"user\");\n      }\n    },\n    login() {\n      this.showLogin = true;\n    },\n    closeLogin() {\n      this.showLogin = false;\n    },\n    capitalize(value) {\n      if (!value) return \"\";\n      value = value.toString();\n      return value.charAt(0).toUpperCase() + value.slice(1);\n    },\n  },\n  created() {\n    this.checkUser();\n  },\n};\n</script>"],"sourceRoot":""}]}