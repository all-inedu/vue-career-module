,IAAI,CAAC,OAAL,EA3BL;AAAA;;AAAA;AA2BZ,4DAAiC;AAAA,cAAxB,IAAwB;AAC/B,UAAA,OAAO,CAAC,GAAR,CAAY,IAAI,CAAC,CAAD,CAAJ,GAAU,IAAV,GAAiB,IAAI,CAAC,CAAD,CAAjC;AACF,SA7BY,CA+BZ;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AA1CY;AAAA;AAAA;AAAA;AAAA;AA2Cb;AAvKM,GArBI;AA8Lb,EAAA,KAAK,EAAE;AACL,IAAA,MADK,kBACE,EADF,EACM;AACT,WAAK,WAAL,CAAiB,EAAE,CAAC,MAAH,CAAU,OAA3B;AACD;AAHI,GA9LM;AAmMb,EAAA,OAnMa,qBAmMH;AACR,QAAI,cAAc,CAAC,OAAf,CAAuB,MAAvB,KAAkC,IAAtC,EAA4C;AAC1C,WAAK,WAAL,GAAmB,IAAI,CAAC,KAAL,CAAW,cAAc,CAAC,OAAf,CAAuB,MAAvB,CAAX,CAAnB;AACF,KAFA,MAEO;AACL,WAAK,WAAL,GAAmB,cAAc,CAAC,OAAf,CAAuB,MAAvB,CAAnB;AACF;;AACA,SAAK,aAAL;AACA,SAAK,cAAL;AACA,SAAK,WAAL,CAAiB,KAAK,MAAL,CAAY,MAAZ,CAAmB,OAApC;AACD;AA5MY,CAAf","sourcesContent":["<template>\n  <div id=\"element\">\n    <transition name=\"fade\">\n      <div class=\"container p-md-3 p-2\">\n        <div class=\"row\">\n          <div class=\"col-md-12\">\n            <div class=\"card\">\n              <div class=\"card-body\">\n                <h3 class=\"m-0\">Element of {{ part.title }}</h3>\n                <hr />\n                <transition name=\"fade\">\n                  <div class=\"row justify-content-center\" v-if=\"!addElement\">\n                    <div class=\"col-md-6\">\n                      <div class=\"border-add pointer\">\n                        <div class=\"container\">\n                          <div class=\"row g-2 align-items-stretch\">\n                            <div class=\"col my-1\">\n                              <div\n                                class=\"border p-3 h-100 border-element\"\n                                @click=\"addForm\"\n                              >\n                                <vue-feather\n                                  class=\"pe-2\"\n                                  type=\"plus-circle\"\n                                ></vue-feather>\n                                <h6 class=\"m-0\">Add an Element</h6>\n                              </div>\n                            </div>\n                          </div>\n                        </div>\n                      </div>\n                    </div>\n                  </div>\n                </transition>\n\n                <transition name=\"fade\">\n                  <div\n                    class=\"row justify-content-center my-3\"\n                    v-if=\"!addElement\"\n                  >\n                    <div class=\"col-md-12\">\n                      <ul class=\"list-group\">\n                        <li class=\"list-group-item\" v-for=\"i in 5\" :key=\"i\">\n                          <span class=\"badge rounded-pill bg-primary me-2\">\n                            {{ i }}\n                          </span>\n                          Lorem ipsum\n                          <div class=\"float-end\">\n                            <span class=\"badge rounded-pill bg-dark me-2\">\n                              Description\n                            </span>\n                          </div>\n                        </li>\n                      </ul>\n                    </div>\n                  </div>\n                </transition>\n\n                <transition name=\"fade\">\n                  <div\n                    class=\"row justify-content-center my-3\"\n                    v-if=\"addElement\"\n                  >\n                    <div class=\"col-md-12 mb-3\">\n                      <div class=\"row align-items-center mb-2\">\n                        <div class=\"col\">\n                          <h5 class=\"text-start\">Title</h5>\n                        </div>\n                      </div>\n                      <hr class=\"my-0 mb-3\" />\n                      <div\n                        class=\"col-md-12 py-2 text-center bg-white sticky-top\"\n                        style=\"top: 93px\"\n                      >\n                        <button\n                          class=\"\n                            btn btn-admin btn-sm\n                            border border-success\n                            mx-1\n                            my-1\n                          \"\n                          @click=\"addElementField('text', element)\"\n                        >\n                          <vue-feather type=\"plus\" size=\"20\"></vue-feather>\n                          Text\n                        </button>\n                        <button\n                          class=\"\n                            btn btn-admin btn-sm\n                            border border-success\n                            mx-1\n                            my-1\n                          \"\n                          @click=\"addElementField('image', element)\"\n                        >\n                          <vue-feather type=\"plus\" size=\"20\"></vue-feather>\n                          Image\n                        </button>\n                        <button\n                          class=\"\n                            btn btn-admin btn-sm\n                            border border-success\n                            mx-1\n                            my-1\n                          \"\n                          @click=\"addElementField('video', element)\"\n                        >\n                          <vue-feather type=\"plus\" size=\"20\"></vue-feather>\n                          Video\n                        </button>\n                        <button\n                          class=\"\n                            btn btn-admin btn-sm\n                            border border-success\n                            mx-1\n                            my-1\n                          \"\n                          @click=\"addElementField('file', element)\"\n                        >\n                          <vue-feather type=\"plus\" size=\"20\"></vue-feather>\n                          Upload File\n                        </button>\n                        <button\n                          class=\"\n                            btn btn-admin btn-sm\n                            border border-success\n                            mx-1\n                            my-1\n                          \"\n                          @click=\"addElementField('multiple', element)\"\n                        >\n                          <vue-feather type=\"plus\" size=\"20\"></vue-feather>\n                          Multiple Choice\n                        </button>\n                        <button\n                          class=\"\n                            btn btn-admin btn-sm\n                            border border-success\n                            mx-1\n                            my-1\n                          \"\n                          @click=\"addElementField('blank', element)\"\n                        >\n                          <vue-feather type=\"plus\" size=\"20\"></vue-feather>\n                          Fill-in the Blanks\n                        </button>\n                        <hr class=\"my-3\" />\n                      </div>\n                      {{ element }}\n                      <form @submit.prevent=\"saveElement\">\n                        <div class=\"row\">\n                          <div\n                            :class=\"\n                              element.data[index].c_element == 'text' ||\n                              element.data[index].c_element == 'multiple' ||\n                              element.data[index].c_element == 'blank' ||\n                              element.data[index].c_element == 'file'\n                                ? 'col-md-12'\n                                : 'col-md-6'\n                            \"\n                            v-for=\"(i, index) in element.data\"\n                            :key=\"index\"\n                          >\n                            <!-- Image  -->\n                            <transition name=\"fade\">\n                              <div\n                                class=\"mb-3\"\n                                v-if=\"element.data[index].c_element == 'image'\"\n                              >\n                                <small class=\"text-primary fw-bold\"\n                                  >Image Element</small\n                                >\n                                <div\n                                  class=\"float-end\"\n                                  @click=\"removeField(index, element.data)\"\n                                >\n                                  <vue-feather\n                                    class=\"pointer\"\n                                    type=\"trash-2\"\n                                    size=\"20\"\n                                    stroke=\"red\"\n                                  ></vue-feather>\n                                </div>\n                                <small class=\"text-muted d-block mb-2\"\n                                  >Insert an image</small\n                                >\n                                <input\n                                  type=\"file\"\n                                  ref=\"file\"\n                                  class=\"form-control\"\n                                  placeholder=\"Select the image\"\n                                  @change=\"previewImage(index)\"\n                                />\n                              </div>\n                            </transition>\n\n                            <!-- Video  -->\n                            <transition name=\"fade\">\n                              <div\n                                class=\"mb-3\"\n                                v-if=\"element.data[index].c_element == 'video'\"\n                              >\n                                <small class=\"text-primary fw-bold\"\n                                  >Video Element</small\n                                >\n                                <div\n                                  class=\"float-end\"\n                                  @click=\"removeField(index, element.data)\"\n                                >\n                                  <vue-feather\n                                    class=\"pointer\"\n                                    type=\"trash-2\"\n                                    size=\"20\"\n                                    stroke=\"red\"\n                                  ></vue-feather>\n                                </div>\n                                <small class=\"text-muted d-block mb-2\">\n                                  Insert a video from YouTube\n                                </small>\n                                <input\n                                  type=\"url\"\n                                  class=\"form-control\"\n                                  placeholder=\"Paste Youtube video link here\"\n                                  v-model=\"element.data[index].value\"\n                                />\n                              </div>\n                            </transition>\n\n                            <!-- Text  -->\n                            <transition name=\"fade\">\n                              <div\n                                class=\"mb-3\"\n                                v-if=\"element.data[index].c_element == 'text'\"\n                              >\n                                <small class=\"text-primary fw-bold\"\n                                  >Text Element</small\n                                >\n                                <div\n                                  class=\"float-end\"\n                                  @click=\"removeField(index, element.data)\"\n                                >\n                                  <vue-feather\n                                    class=\"pointer\"\n                                    type=\"trash-2\"\n                                    size=\"20\"\n                                    stroke=\"red\"\n                                  ></vue-feather>\n                                </div>\n                                <small class=\"text-muted d-block\">\n                                  Type your description below\n                                </small>\n                                <div class=\"my-2\"></div>\n                                <editor\n                                  v-if=\"element.data[index].c_element == 'text'\"\n                                  v-model=\"element.data[index].description\"\n                                  api-key=\"h7t62ozvqkx2ifkeh051fsy3k9irz7axx1g2zitzpbaqfo8m\"\n                                  :init=\"{\n                                    height: 400,\n                                    menubar: true,\n                                    inline: false,\n                                    plugins:\n                                      'print preview importcss searchreplace autolink autosave save directionality visualblocks visualchars fullscreen image link media  template codesample table charmap hr pagebreak nonbreaking anchor  insertdatetime advlist lists wordcount   textpattern noneditable help charmap quickbars  emoticons',\n                                    toolbar:\n                                      'undo redo | formatselect | bold italic backcolor | \\\n           alignleft aligncenter alignright alignjustify | \\\n           bullist numlist outdent indent | removeformat | help',\n                                  }\"\n                                />\n                              </div>\n                            </transition>\n\n                            <!-- Upload file  -->\n                            <transition name=\"fade\">\n                              <div\n                                class=\"mb-3\"\n                                v-if=\"element.data[index].c_element == 'file'\"\n                              >\n                                <small class=\"text-primary fw-bold\"\n                                  >Upload File Element</small\n                                >\n                                <div\n                                  class=\"float-end\"\n                                  @click=\"removeField(index, element.data)\"\n                                >\n                                  <vue-feather\n                                    class=\"pointer\"\n                                    type=\"trash-2\"\n                                    size=\"20\"\n                                    stroke=\"red\"\n                                  ></vue-feather>\n                                </div>\n                                <small class=\"text-muted d-block\"\n                                  >Type your question below</small\n                                >\n                                <div class=\"my-2\"></div>\n                                <div class=\"mb-3\">\n                                  <editor\n                                    v-model=\"element.data[index].value\"\n                                    api-key=\"h7t62ozvqkx2ifkeh051fsy3k9irz7axx1g2zitzpbaqfo8m\"\n                                    :init=\"{\n                                      height: 400,\n                                      menubar: true,\n                                      inline: false,\n                                      plugins:\n                                        'print preview importcss searchreplace autolink autosave save directionality visualblocks visualchars fullscreen image link media  template codesample table charmap hr pagebreak nonbreaking anchor  insertdatetime advlist lists wordcount   textpattern noneditable help charmap quickbars  emoticons',\n                                      toolbar:\n                                        'undo redo | formatselect | bold italic backcolor | \\\n           alignleft aligncenter alignright alignjustify | \\\n           bullist numlist outdent indent | removeformat | help',\n                                    }\"\n                                  />\n                                </div>\n                                <small class=\"text-muted d-block mb-2\">\n                                  Upload your worksheet below\n                                </small>\n                                <input\n                                  type=\"file\"\n                                  ref=\"file\"\n                                  class=\"form-control\"\n                                  placeholder=\"Select the file\"\n                                  @change=\"previewFile(index)\"\n                                />\n                              </div>\n                            </transition>\n\n                            <!-- Multiple choice -->\n                            <transition name=\"fade\">\n                              <div\n                                class=\"mb-3\"\n                                v-if=\"\n                                  element.data[index].c_element == 'multiple'\n                                \"\n                              >\n                                <small class=\"text-primary fw-bold\"\n                                  >Multiple Choice Element</small\n                                >\n                                <div\n                                  class=\"float-end\"\n                                  @click=\"removeField(index, element.data)\"\n                                >\n                                  <vue-feather\n                                    class=\"pointer\"\n                                    type=\"trash-2\"\n                                    size=\"20\"\n                                    stroke=\"red\"\n                                  ></vue-feather>\n                                </div>\n                                <small class=\"text-muted d-block\"\n                                  >Type your question below</small\n                                >\n                                <div class=\"my-2\"></div>\n                                <editor\n                                  v-model=\"element.data[index].value\"\n                                  api-key=\"h7t62ozvqkx2ifkeh051fsy3k9irz7axx1g2zitzpbaqfo8m\"\n                                  :init=\"{\n                                    height: 400,\n                                    menubar: true,\n                                    inline: false,\n                                    plugins:\n                                      'print preview importcss searchreplace autolink autosave save directionality visualblocks visualchars fullscreen image link media  template codesample table charmap hr pagebreak nonbreaking anchor  insertdatetime advlist lists wordcount   textpattern noneditable help charmap quickbars  emoticons',\n                                    toolbar:\n                                      'undo redo | formatselect | bold italic backcolor | \\\n           alignleft aligncenter alignright alignjustify | \\\n           bullist numlist outdent indent | removeformat | help',\n                                  }\"\n                                />\n                                <div class=\"my-2 mt-3\">\n                                  <small class=\"text-muted d-block\"\n                                    >Type the answer options below and press the\n                                    radio button for the correct answer</small\n                                  >\n                                </div>\n                                <div class=\"row\">\n                                  <div\n                                    class=\"col-md-6 align-items-center\"\n                                    v-for=\"(i, key) in 4\"\n                                    :key=\"key\"\n                                  >\n      