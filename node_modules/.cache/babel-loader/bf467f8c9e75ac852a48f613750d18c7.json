r:\n                                        'undo redo | formatselect | bold italic backcolor | \\\n           alignleft aligncenter alignright alignjustify | \\\n           bullist numlist outdent indent | removeformat | help',\n                                    }\"\n                                  />\n                                </div>\n                                <div class=\"row\">\n                                  <small class=\"text-muted d-block\"\n                                    >Mark an answer as correct, if the\n                                    answer</small\n                                  >\n                                  <div class=\"col-4\">\n                                    <select\n                                      class=\"form-select\"\n                                      :change=\"\n                                        typeBlank(\n                                          element.data[index].type_blank\n                                        )\n                                      \"\n                                      v-model=\"element.data[index].type_blank\"\n                                    >\n                                      <option value=\"is exactly\">\n                                        is exactly\n                                      </option>\n                                      <option value=\"is number\">\n                                        exact number\n                                      </option>\n                                    </select>\n                                  </div>\n                                  <div class=\"col-8\">\n                                    <input\n                                      :type=\"typeInput\"\n                                      class=\"form-control\"\n                                      v-model=\"\n                                        element.data[index].answer_correct\n                                      \"\n                                    />\n                                  </div>\n                                </div>\n                              </div>\n                            </transition>\n                          </div>\n\n                          <div class=\"col-md-12\">\n                            <div class=\"float-start\">\n                              <button\n                                type=\"button\"\n                                class=\"btn btn-admin btn-warning btn-sm mx-0\"\n                                @click=\"closeForm\"\n                              >\n                                Back to List\n                              </button>\n                            </div>\n                            <div class=\"float-end\" v-if=\"element.data\">\n                              <button\n                                class=\"btn btn-admin btn-primary btn-sm mx-0\"\n                                @click=\"closeForm\"\n                              >\n                                Save an Element\n                              </button>\n                            </div>\n                          </div>\n                        </div>\n                      </form>\n                    </div>\n                  </div>\n                </transition>\n                <div class=\"mt-3\" v-if=\"!addElement\">\n                  <hr />\n                  <div class=\"float-start\">\n                    <button\n                      @click=\"previous\"\n                      class=\"btn btn-admin btn-warning mx-0\"\n                    >\n                      <vue-feather\n                        class=\"pe-2\"\n                        type=\"arrow-left-circle\"\n                      ></vue-feather>\n                      Return\n                    </button>\n                  </div>\n                </div>\n              </div>\n            </div>\n          </div>\n        </div>\n      </div>\n    </transition>\n  </div>\n</template>\n<script>\nimport VueFeather from \"vue-feather\";\nimport Swal from \"sweetalert2\";\nimport axios from \"axios\";\n\nexport default {\n  name: \"element\",\n  components: {\n    VueFeather,\n  },\n  data() {\n    return {\n      api_url: \"https://api-cm.all-inedu.com/api/v1/\",\n      addElement: false,\n      typeInput: \"text\",\n      userSession: [],\n      module: [],\n      outline: [],\n      part: [],\n      element: {\n        part_id: \"\",\n        module_id: this.$route.params.module_id,\n        data: [],\n      },\n    };\n  },\n  methods: {\n    previous() {\n      this.$emit(\"check-section\", 3);\n    },\n    getModuleData() {\n      axios\n        .get(this.api_url + \"module/\" + this.$route.params.module_id, {\n          headers: {\n            Authorization: \"Bearer \" + this.userSession.data.token,\n          },\n        })\n        .then((response) => {\n          this.module = response.data.data[0];\n        })\n        .catch((error) => {\n          console.log(error.response);\n        });\n    },\n    getOutlineData() {\n      axios\n        .get(this.api_url + \"detail/outline/\" + this.$route.params.outline_id, {\n          headers: {\n            Authorization: \"Bearer \" + this.userSession.data.token,\n          },\n        })\n        .then((response) => {\n          this.outline = response.data.outline;\n        })\n        .catch((error) => {\n          console.log(error.response);\n        });\n    },\n    getPartData(id) {\n      axios\n        .get(this.api_url + \"detail/part/\" + id, {\n          headers: {\n            Authorization: \"Bearer \" + this.userSession.data.token,\n          },\n        })\n        .then((response) => {\n          this.part = response.data.part;\n          this.element.part_id = this.part.id;\n        })\n        .catch((error) => {\n          console.log(error.response);\n        });\n    },\n    getElementData() {\n      //\n    },\n    addForm() {\n      this.addElement = true;\n    },\n    closeForm() {\n      this.addElement = false;\n    },\n    addElementField(name, element) {\n      if (name == \"multiple\") {\n        element.data.push({\n          c_element: name,\n          answer_in_array: [\n            {\n              answer: \"\",\n              value: \"\",\n            },\n            {\n              answer: \"\",\n              value: \"\",\n            },\n            {\n              answer: \"\",\n              value: \"\",\n            },\n            {\n              answer: \"\",\n              value: \"\",\n            },\n          ],\n        });\n      } else {\n        element.data.push({\n          c_element: name,\n        });\n      }\n      // console.log(this.element);\n    },\n    removeField(index, element) {\n      Swal.fire({\n        html: \"Are you sure to delete this element?\",\n        showDenyButton: false,\n        showCancelButton: true,\n        confirmButtonText: \"Yes\",\n      }).then((result) => {\n        /* Read more about isConfirmed, isDenied below */\n        if (result.isConfirmed) {\n          element.splice(index, 1);\n        }\n      });\n    },\n    typeBlank(type) {\n      if (type == \"is exactly\") {\n        this.typeInput = \"text\";\n      } else {\n        this.typeInput = \"number\";\n      }\n    },\n    previewImage(index) {\n      const file = this.$refs.file;\n      this.element.data[index].file = file.files[0];\n    },\n    previewFile(index) {\n      const file = this.$refs.file;\n      this.element.data[index].file = file.files[0];\n    },\n    checkAnswer(i, k) {\n      let n = 0;\n      for (n; n < 4; n++) {\n        if (n == k) {\n          this.element[i].answer_in_array[k].value = true;\n        } else {\n          this.element[i].answer_in_array[n].value = false;\n        }\n      }\n    },\n    saveElement() {\n      this.addElement = true;\n      console.log(this.element);\n      let form = new FormData();\n      form.append(\"part_id\", this.element.part_id);\n      form.append(\"module_id\", this.element.module_id);\n\n      for (let i = 0; i < this.element.data.length; i++) {\n        if (this.element.data[i].c_element == \"text\") {\n          form.append(\n            \"data[\" + i + \"] : c_element\",\n            this.element.data[i].c_element\n          );\n          form.append(\n            \"data[\" + i + \"] : description\",\n            this.element.data[i].description\n          );\n        } else if (this.element.data[i].c_element == \"image\") {\n          form.append(\n            \"data[\" + i + \"] : c_element\",\n            this.element.data[i].c_element\n          );\n          form.append(\"data[\" + i + \"] : file\", this.element.data[i].file);\n        }\n      }\n\n      // Display the key/value pairs\n      for (var pair of form.entries()) {\n        console.log(pair[0] + \", \" + pair[1]);\n      }\n\n      // axios\n      //   .post(this.api_url + \"element/\", this.element, {\n      //     headers: {\n      //       Authorization: \"Bearer \" + this.userSession.data.token,\n      //     },\n      //   })\n      //   .then((response) => {\n      //     console.lof(response);\n      //   })\n      //   .catch((error) => {\n      //     console.log(error);\n      //   });\n    },\n  },\n  watch: {\n    $route(to) {\n      this.getPartData(to.params.part_id);\n    },\n  },\n  created() {\n    if (sessionStorage.getItem(\"user\") != null) {\n      this.userSession = JSON.parse(sessionStorage.getItem(\"user\"));\n    } else {\n      this.userSession = sessionStorage.getItem(\"user\");\n    }\n    this.getModuleData();\n    this.getOutlineData();\n    this.getPartData(this.$route.params.part_id);\n  },\n};\n</script>\n<style scoped>\n.fade-enter-active {\n  transition: opacity 0.9s ease;\n}\n\n.fade-enter-from {\n  opacity: 0;\n}\n\n.border-add {\n  border: 3px dotted #919191;\n  text-align: center;\n  padding: 30px 0;\n  transition: all 0.3s ease;\n}\n\n.border-add:hover {\n  border: 3px solid #b6e48b;\n}\n\n.border-element:hover {\n  border: 1px solid #5073e9 !important;\n}\n</style>"],"sourceRoot":""}]}