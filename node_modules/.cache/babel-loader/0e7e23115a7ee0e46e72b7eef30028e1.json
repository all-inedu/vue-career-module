,EAI0F,CAJ1F,C,iCAoBG,EAZQ,OAAO,CAAC,gBAAR,IAAwB,M,kBAAnC,mBAAA,CAWM,KAXN,EAWM,WAXN,EAWM,CAVJ,mBAAA,CASI,GATJ,EASI;AARD,cAAA,IAAI,EAA0C,IAAA,CAAA,SAAA,GAAmD,OAAO,CAAC,cAAR,CAAsB,CAAtB,EAA0B,SAQ1H;AAJF,cAAA,KAAK,EAAC,qCAIJ;AAHF,cAAA,QAAQ,EAAR;AAGE,aATJ,EAOC,kBAPD,EASA,CATA,EASA,WATA,CAUI,CAXN,C,iCAYG,CAxBL,C,CATF,C;WAkCK,CAlCL,E,GAAA,CAmCM,EAxCR,CAyCI,CA1CN,CA2CG,CA5CL,C,CAPF,C;SAoDK,CApDL,E,GAAA,CAR0D,EAA5D,C;OA6DQ,CA7DR,E,GAAA,CA8DM,EA7ER,CA8EI,CA/EN,CAgFI,CAlGN,CAmGI,CApGN,CAqGI,CAtGN,CAuGI,CAxGN,C,iCAwGM,CAxGN;AAAA,K,CADqB;;AAAA,GAAvB,CAgHI,EALJ,YAAA,CAIa,WAJb,EAIa;AAJD,IAAA,IAAI,EAAC;AAIJ,GAJb,EAAuB;sBACrB;AAAA,aAEM,CAFkB,KAAA,CAAA,I,kBAAxB,mBAAA,CAEM,KAFN,EAAA,WAAA,EAEM,CADJ,YAAA,CAAqB,mBAArB,CACI,CAFN,C,iCAEM,CAFN;AAAA,K,CADqB;;AAAA,GAAvB,CAKI,CA1HN,C","sourcesContent":["<template>\r\n  <div id=\"answer\">\r\n    <v-navbar></v-navbar>\r\n    <div class=\"container mt-5\" v-if=\"!show\">\r\n      <div class=\"row justify-content-center\">\r\n        <div class=\"col-md-4 text-center\">\r\n          <vue-feather type=\"loader\" animation=\"spin\" size=\"50\"></vue-feather>\r\n        </div>\r\n      </div>\r\n    </div>\r\n\r\n    <transition name=\"fade\">\r\n      <div class=\"container mt-4 answer\" v-if=\"show\">\r\n        <div class=\"row mx-2\">\r\n          <div class=\"card shadow\">\r\n            <div class=\"card-body\">\r\n              <h5 class=\"mb-3\">\r\n                <vue-feather\r\n                  type=\"arrow-left\"\r\n                  class=\"me-3 pointer\"\r\n                  @click=\"backToDashboard\"\r\n                >\r\n                </vue-feather>\r\n                {{ module_data.module_name }}\r\n              </h5>\r\n              <button\r\n                class=\"btn-warning float-end btn-sm rounded-pill\"\r\n                @click=\"download\"\r\n              >\r\n                Download\r\n              </button>\r\n              <br />\r\n              <div class=\"mb-3\"></div>\r\n              <div class=\"table-responsive\">\r\n                <table class=\"table table-bordered align-middle\">\r\n                  <thead>\r\n                    <tr class=\"text-center\">\r\n                      <th class=\"border-end\" width=\"1%\">No</th>\r\n                      <th width=\"30%\">Part Name</th>\r\n                      <th>Question / Answer</th>\r\n                    </tr>\r\n                  </thead>\r\n                  <tbody v-if=\"outline_length == 0\">\r\n                    <tr class=\"text-center\">\r\n                      <td colspan=\"3\" class=\"py-4\">\r\n                        This module has no questions\r\n                      </td>\r\n                    </tr>\r\n                  </tbody>\r\n                  <tbody v-for=\"outline in module_data.outlines\" :key=\"outline\">\r\n                    <tr class=\"text-center bg-secondary text-white\">\r\n                      <td colspan=\"3\">\r\n                        <strong>\r\n                          {{ outline.name }}\r\n                        </strong>\r\n                      </td>\r\n                    </tr>\r\n                    <tr\r\n                      class=\"text-center\"\r\n                      v-for=\"(part, index) in outline.parts\"\r\n                      :key=\"index\"\r\n                    >\r\n                      <td class=\"border\">{{ index + 1 }}</td>\r\n                      <td class=\"border\">{{ part.title }}</td>\r\n                      <td class=\"border\">\r\n                        <div class=\"table-responsive\">\r\n                          <table class=\"table border mb-0\">\r\n                            <tr class=\"bg-light\">\r\n                              <th class=\"border py-2\">Question</th>\r\n                              <th class=\"border\" width=\"40%\">Answer</th>\r\n                            </tr>\r\n                            <tr\r\n                              class=\"border-1\"\r\n                              v-for=\"element in part.elements\"\r\n                              :key=\"element\"\r\n                            >\r\n                              <td\r\n                                class=\"border text-start\"\r\n                                v-html=\"element.question\"\r\n                              ></td>\r\n                              <td class=\"border\">\r\n                                <div v-if=\"element.category_element == 'blank'\">\r\n                                  {{ element.answersdetails[0].answer }}\r\n                                </div>\r\n                                <div\r\n                                  v-if=\"element.category_element == 'multiple'\"\r\n                                >\r\n                                  {{\r\n                                    element.answersdetails[0]\r\n                                      .answer_from_multiple\r\n                                  }}\r\n                                </div>\r\n                                <div v-if=\"element.category_element == 'file'\">\r\n                                  <a\r\n                                    :href=\"\r\n                                      $base_url +\r\n                                      element.answersdetails[0].file_path\r\n                                    \"\r\n                                    class=\"btn btn-primary btn-sm rounded-pill\"\r\n                                    download\r\n                                  >\r\n                                    Your Worksheet\r\n                                  </a>\r\n                                </div>\r\n                              </td>\r\n                            </tr>\r\n                          </table>\r\n                        </div>\r\n                      </td>\r\n                    </tr>\r\n                  </tbody>\r\n                </table>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </transition>\r\n    <transition name=\"fade\">\r\n      <div class=\"mt-4\" v-if=\"show\">\r\n        <v-footer></v-footer>\r\n      </div>\r\n    </transition>\r\n  </div>\r\n</template>\r\n<script>\r\nimport hNavbar from \"@/components/User/Navbar\";\r\nimport hFooter from \"@/components/Home/Footer\";\r\nimport VueFeather from \"vue-feather\";\r\n\r\nexport default {\r\n  name: \"answer\",\r\n  components: {\r\n    \"v-navbar\": hNavbar,\r\n    \"v-footer\": hFooter,\r\n    VueFeather,\r\n  },\r\n  data() {\r\n    return {\r\n      user: [],\r\n      show: false,\r\n      module_data: [],\r\n      outline_length: \"\",\r\n    };\r\n  },\r\n  methods: {\r\n    backToDashboard() {\r\n      this.$router.push({ path: \"/user/dashboard\" });\r\n    },\r\n    getModuleData(slug) {\r\n      this.$axios\r\n        .get(this.$api_url + \"act/view/answer/\" + slug, {\r\n          headers: {\r\n            Authorization: \"Bearer \" + this.user.token,\r\n          },\r\n        })\r\n        .then((response) => {\r\n          this.module_data = response.data;\r\n          this.outline_length = this.module_data.outlines.length;\r\n          // console.log(this.module_data);\r\n        })\r\n        .catch((error) => {\r\n          console.log(error);\r\n        });\r\n    },\r\n    download() {\r\n      window.open(\r\n        \"https://api-cm.all-inedu.com/api/v1/module/\" +\r\n          this.$route.params.slug +\r\n          \"/answer/user/\" +\r\n          this.user.id +\r\n          \"/download/\",\r\n        \"_blank\"\r\n      );\r\n    },\r\n  },\r\n  created() {\r\n    document.title = \"Module Answer\";\r\n    this.user = this.$auth.check();\r\n\r\n    if (this.$route.params.slug) {\r\n      // console.log(this.$route.params.slug);\r\n      this.getModuleData(this.$route.params.slug);\r\n    }\r\n\r\n    setTimeout(() => {\r\n      this.show = true;\r\n    }, 500);\r\n  },\r\n};\r\n</script>\r\n<style>\r\n.answer {\r\n  min-height: 47vh;\r\n}\r\n\r\np {\r\n  margin-bottom: 0px;\r\n}\r\n</style>"],"sourceRoot":""}]}