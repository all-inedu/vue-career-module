D6B,OAD1C,EAMG,IANH,CAMQ,UAAC,QAAD,EAAc;AAClB;AACA;AACA,QAAA,KAAI,CAAC,MAAL,CAAY,aAAZ,GAA4B,QAAQ,CAAC,IAAT,CAAc,UAAd,CAAyB,MAArD;AACA,QAAA,KAAI,CAAC,MAAL,CAAY,OAAZ,GAAsB,QAAQ,CAAC,IAAT,CAAc,UAApC,CAJkB,CAMlB;;AACA,QAAA,KAAI,CAAC,MAAL,CAAY,eAAZ,GAA8B,QAAQ,CAAC,IAAT,CAAc,SAAd,CAAwB,MAAtD;AACA,QAAA,KAAI,CAAC,MAAL,CAAY,SAAZ,GAAwB,QAAQ,CAAC,IAAT,CAAc,SAAtC;;AAEA,QAAA,KAAI,CAAC,KAAL,CAAW,YAAX,EAAyB,KAAI,CAAC,MAA9B;AACD,OAjBH,EAkBG,KAlBH,CAkBS,UAAC,KAAD,EAAW;AAChB,QAAA,OAAO,CAAC,GAAR,CAAY,KAAZ;AACD,OApBH;AAqBD,KA3BM;AA4BP,IAAA,cA5BO,0BA4BQ,IA5BR,EA4Bc,UA5Bd,EA4B0B;AAC/B,UAAI,CAAC,UAAL,EAAiB;AACf,aAAK,OAAL,CAAa,IAAb,CAAkB;AAAE,UAAA,IAAI,EAAE,aAAa;AAArB,SAAlB;AACF,OAFA,MAEO;AACL,aAAK,OAAL,CAAa,IAAb,CAAkB;AAChB,UAAA,IAAI,EAAE,aAAa,IAAb,GAAoB,GAApB,GAA0B;AADhB,SAAlB;AAGF;AACD,KApCM;AAqCP,IAAA,UArCO,sBAqCI,IArCJ,EAqCU;AACf,WAAK,OAAL,CAAa,IAAb,CAAkB;AAChB,QAAA,IAAI,EAAE,yBAAyB;AADf,OAAlB;AAGD;AAzCM,GAhBI;AA2Db,EAAA,OA3Da,qBA2DH;AACR,SAAK,IAAL,GAAY,KAAK,KAAL,CAAW,SAAX,EAAZ;AACA,SAAK,gBAAL;AACD;AA9DY,CAAf","sourcesContent":["<template>\n  <div id=\"activity\">\n    <div class=\"row mt-3 g-0\">\n      <div class=\"col-md-12 mx-0\">\n        <div class=\"card shadow\">\n          <div class=\"card-body\">\n            <h4>My Activity</h4>\n            <div class=\"row\">\n              <div class=\"col-md-6\">\n                <div class=\"card\">\n                  <div class=\"card-body\">\n                    <h6 class=\"text-center\">Ongoing</h6>\n                    <hr class=\"mt-0\" />\n                    <div\n                      class=\"card card-module mb-3\"\n                      v-for=\"c in module.ongoing\"\n                      :key=\"c\"\n                    >\n                      <div class=\"row g-0\">\n                        <div class=\"col-md-4 bg-ongoing p-3\">\n                          <div class=\"module-name\">\n                            {{ c.module_name }}\n                          </div>\n                          <div class=\"border-top border-white module-category\">\n                            <small>\n                              {{ c.category_name }}\n                            </small>\n                          </div>\n                          <div class=\"module-taken\">\n                            {{ formatDate(c.taken_module) }}\n                          </div>\n                        </div>\n                        <div class=\"col-md-8 p-3\">\n                          <div class=\"module-progress\">\n                            <div class=\"progress\" style=\"height: 8px\">\n                              <div\n                                class=\"progress-bar\"\n                                role=\"progressbar\"\n                                :style=\"'width:' + c.percentage + '%'\"\n                                aria-valuemax=\"100\"\n                              ></div>\n                            </div>\n                          </div>\n                          <div class=\"row module-outline\">\n                            <div class=\"col-6 mb-3 text-start\">\n                              {{ c.section_name }}\n                            </div>\n                            <div class=\"col-6 text-end\">\n                              {{ Math.round(c.percentage) }} %\n                            </div>\n                          </div>\n                          <div class=\"module-part\">\n                            {{ c.current_part_name }}\n                          </div>\n                          <button\n                            class=\"\n                              btn btn-sm btn-primary\n                              rounded-pill\n                              float-end\n                              mx-0\n                            \"\n                            @click=\"continueModule(c.slug, c.outline_id)\"\n                          >\n                            Continue\n                            <vue-feather\n                              type=\"arrow-right\"\n                              size=\"18\"\n                            ></vue-feather>\n                          </button>\n                        </div>\n                      </div>\n                    </div>\n                    <div\n                      class=\"text-center mt-3 text-muted\"\n                      v-if=\"module.ongoing_count == 0\"\n                    >\n                      You haven't taken the module\n                    </div>\n                  </div>\n                </div>\n              </div>\n              <div class=\"col-md-6\">\n                <div class=\"card\">\n                  <div class=\"card-body\">\n                    <h6 class=\"text-center\">Completed</h6>\n                    <hr class=\"mt-0\" />\n                    <div\n                      class=\"card card-module mb-3\"\n                      v-for=\"c in module.completed\"\n                      :key=\"c\"\n                    >\n                      <div class=\"row g-0\">\n                        <div class=\"col-md-4 bg-completed p-3\">\n                          <div class=\"module-name\">\n                            {{ c.module_name }}\n                          </div>\n                          <div class=\"border-top border-white module-category\">\n                            <small>\n                              {{ c.category_name }}\n                            </small>\n                          </div>\n                          <div class=\"module-taken\">\n                            {{ formatDate(c.taken_date) }}\n                          </div>\n                        </div>\n                        <div class=\"col-md-8 p-3\">\n                          <div class=\"module-progress\">\n                            <div class=\"progress\" style=\"height: 8px\">\n                              <div\n                                class=\"progress-bar bg-success\"\n                                role=\"progressbar\"\n                                :style=\"'width:' + c.percentage + '%'\"\n                                aria-valuemax=\"100\"\n                              ></div>\n                            </div>\n                          </div>\n                          <div class=\"row module-outline\">\n                            <div class=\"col-8 mb-3 text-start\">\n                              {{ c.section_name }}\n                            </div>\n                            <div class=\"col-4 text-end\">\n                              {{ Math.round(c.percentage) }} %\n                            </div>\n                          </div>\n                          <div class=\"module-part\">\n                            {{ c.current_part_name }}\n                          </div>\n                          <div class=\"float-md-start text-center mb-2\">\n                            <button\n                              class=\"btn btn-sm btn-warning rounded-pill mx-0\"\n                              @click=\"viewAnswer(c.slug)\"\n                            >\n                              <vue-feather\n                                type=\"clipboard\"\n                                class=\"me-2\"\n                                size=\"20\"\n                              ></vue-feather>\n                              View Answer\n                            </button>\n                          </div>\n                          <div class=\"float-md-end text-center\">\n                            <button\n                              class=\"btn btn-sm btn-primary rounded-pill mx-0\"\n                              @click=\"continueModule(c.slug)\"\n                            >\n                              <vue-feather\n                                type=\"book-open\"\n                                class=\"me-2\"\n                                size=\"20\"\n                              ></vue-feather>\n                              Read Again\n                            </button>\n                          </div>\n                        </div>\n                      </div>\n                    </div>\n                    <div\n                      class=\"text-center mt-3 text-muted\"\n                      v-if=\"module.completed_count == 0\"\n                    >\n                      You haven't taken the module\n                    </div>\n                  </div>\n                </div>\n              </div>\n            </div>\n          </div>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n<script>\nimport VueFeather from \"vue-feather\";\nimport moment from \"moment\";\nexport default {\n  name: \"activity\",\n  components: {\n    VueFeather,\n  },\n  data() {\n    return {\n      user: [],\n      module: {\n        ongoing_count: \"\",\n        ongoing: [],\n        completed_count: \"\",\n        completed: [],\n      },\n    };\n  },\n  methods: {\n    formatDate(date) {\n      return moment(date).format(\"LLL\");\n    },\n    getOngoingModule() {\n      // console.log(this.user);\n      this.$axios\n        .get(this.$api_url + \"act/latest/read\", {\n          headers: {\n            Authorization: \"Bearer \" + this.user.token,\n          },\n        })\n        .then((response) => {\n          // console.log(response.data);\n          // Onprogress Module\n          this.module.ongoing_count = response.data.onprogress.length;\n          this.module.ongoing = response.data.onprogress;\n\n          // Completed Module\n          this.module.completed_count = response.data.completed.length;\n          this.module.completed = response.data.completed;\n\n          this.$emit(\"check-data\", this.module);\n        })\n        .catch((error) => {\n          console.log(error);\n        });\n    },\n    continueModule(slug, outline_id) {\n      if (!outline_id) {\n        this.$router.push({ path: \"/module/\" + slug });\n      } else {\n        this.$router.push({\n          path: \"/module/\" + slug + \"/\" + outline_id,\n        });\n      }\n    },\n    viewAnswer(slug) {\n      this.$router.push({\n        path: \"/user/module/answer/\" + slug,\n      });\n    },\n  },\n  created() {\n    this.user = this.$auth.user_data();\n    this.getOngoingModule();\n  },\n};\n</script>\n<style>\n.bg-ongoing {\n  background: rgb(204, 122, 14);\n  color: #fff;\n  border-radius: 10px 0 0 10px;\n}\n\n.bg-completed {\n  background: #2c265e;\n  color: #fff;\n  border-radius: 10px 0 0 10px;\n}\n\n.card-module {\n  border-radius: 10px;\n}\n\n.module-name {\n  font-size: 1.1em;\n  font-weight: bold;\n}\n\n.module-category {\n  font-size: 0.9em;\n  line-height: 13px;\n  padding-top: 5px;\n  margin-bottom: 20px;\n}\n\n.module-taken {\n  position: absolute;\n  bottom: 15px;\n  font-size: 0.7em;\n}\n\n.module-outline {\n  margin-top: 5px;\n}\n\n.module-part {\n  margin-top: -15px;\n  margin-bottom: 20px;\n  position: relative;\n  z-index: 10;\n  font-size: 1.1em;\n  font-weight: bold;\n  color: rgb(53, 53, 53);\n}\n\n.module-percentage {\n  position: absolute;\n  top: 12px;\n  right: 12px;\n  font-size: 3em;\n  font-weight: bold;\n  z-index: 9;\n  color: rgba(53, 53, 53, 0.116);\n}\n\n@media screen and (max-width: 600px) {\n  .module-taken {\n    margin-top: 20px;\n    position: relative;\n  }\n\n  .bg-ongoing,\n  .bg-completed {\n    border-radius: 10px 10px 0 0;\n  }\n\n  .module-percentage {\n    top: 10%;\n    color: rgba(228, 226, 226, 0.116);\n  }\n}\n</style>"],"sourceRoot":""}]}