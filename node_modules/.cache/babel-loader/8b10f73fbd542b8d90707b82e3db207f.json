idebarToggle\"\r\n              menu=\"user\"\r\n              submenu=\"user\"\r\n            ></AdminSidebar>\r\n          </div>\r\n        </transition>\r\n        <div :class=\"header\">\r\n          <AdminHeader :display=\"sidebarToggle\"></AdminHeader>\r\n          <div class=\"container p-md-3 p-2\">\r\n            <div class=\"row\">\r\n              <div class=\"col-md-12\"></div>\r\n\r\n              <div class=\"col-md-12\">\r\n                <div class=\"row\">\r\n                  <div class=\"col-md-5\">\r\n                    <div class=\"input-group\">\r\n                      <select class=\"form-select w-25\" v-model=\"filter\">\r\n                        <option value=\"\">Filter</option>\r\n                        <option value=\"name\">Name</option>\r\n                        <option value=\"email\">Email</option>\r\n                      </select>\r\n                      <input\r\n                        v-model=\"search\"\r\n                        @keyup.enter=\"searchProcess\"\r\n                        type=\"text\"\r\n                        class=\"form-control w-75\"\r\n                        placeholder=\"Search\"\r\n                      />\r\n                    </div>\r\n                  </div>\r\n                  <div class=\"col-md-7 text-end\">\r\n                    <vue-feather\r\n                      @click=\"refreshProcess\"\r\n                      type=\"refresh-cw\"\r\n                      class=\"me-3 pointer\"\r\n                    ></vue-feather>\r\n                    <div class=\"form-check d-inline-block form-switch\">\r\n                      <label class=\"form-check-label\">Is verified?</label>\r\n                      <input\r\n                        class=\"form-check-input\"\r\n                        type=\"checkbox\"\r\n                        role=\"switch\"\r\n                        v-model=\"is_verify\"\r\n                        @change=\"verifyProcess\"\r\n                      />\r\n                    </div>\r\n                  </div>\r\n                </div>\r\n                <hr class=\"my-2\" />\r\n              </div>\r\n\r\n              <div class=\"col-md-12 mt-1\">\r\n                <transition name=\"fade\">\r\n                  <div class=\"row row-cols-md-3\" v-if=\"showing\">\r\n                    <div class=\"col-md-12\" v-if=\"userList.from\">\r\n                      <small\r\n                        >Showing {{ userList.from }} to {{ userList.to }} of\r\n                        {{ userList.total }} users</small\r\n                      >\r\n                    </div>\r\n                    <div\r\n                      class=\"col-12 mb-md-4 mb-2\"\r\n                      v-for=\"item in userList.data\"\r\n                      :key=\"item.id\"\r\n                    >\r\n                      <div\r\n                        class=\"card h-100 card-list\"\r\n                        @click=\"goToDetail(item.id)\"\r\n                      >\r\n                        <div class=\"card-body\">\r\n                          <div class=\"row align-items-center\">\r\n                            <div class=\"col-3 text-center\">\r\n                              <vue-feather type=\"user\" size=\"50\"></vue-feather>\r\n                            </div>\r\n                            <div class=\"col-9\">\r\n                              <h6 class=\"my-0 mb-2\">\r\n                                <b class=\"text-uppercase\">\r\n                                  {{ item.first_name }} {{ item.last_name }}\r\n                                  <vue-feather\r\n                                    v-if=\"item.is_verified == 1\"\r\n                                    type=\"check\"\r\n                                    stroke=\"green\"\r\n                                  ></vue-feather>\r\n                                </b>\r\n                              </h6>\r\n                              <p class=\"card-label text-primary\">\r\n                                {{ item.email }}\r\n                              </p>\r\n                              <p class=\"card-label\">\r\n                                {{ item.phone_number }} |\r\n                                {{ formatDate(item.birthday) }}\r\n                              </p>\r\n                            </div>\r\n                          </div>\r\n                        </div>\r\n                        <div class=\"card-footer\">\r\n                          <div class=\"float-start\">\r\n                            <label class=\"join-date\">\r\n                              <vue-feather\r\n                                type=\"calendar\"\r\n                                size=\"20\"\r\n                                class=\"ms-2\"\r\n                              ></vue-feather>\r\n                              Join date on {{ formatDate(item.created_at) }}\r\n                            </label>\r\n                          </div>\r\n                          <div class=\"float-end\">\r\n                            <vue-feather\r\n                              type=\"arrow-right\"\r\n                              size=\"20\"\r\n                              class=\"ms-2\"\r\n                            ></vue-feather>\r\n                          </div>\r\n                        </div>\r\n                      </div>\r\n                    </div>\r\n                  </div>\r\n                </transition>\r\n              </div>\r\n\r\n              <div\r\n                class=\"col-md-12 mt-3\"\r\n                v-if=\"\r\n                  showing && userList.data.length > 0 && userList.last_page > 1\r\n                \"\r\n              >\r\n                <nav aria-label=\"Page navigation example\">\r\n                  <ul class=\"pagination justify-content-center\">\r\n                    <li\r\n                      class=\"page-item\"\r\n                      v-if=\"userList.current_page != 1\"\r\n                      @click=\"userPage(userList.current_page - 1)\"\r\n                    >\r\n                      <a class=\"page-link\">Previous</a>\r\n                    </li>\r\n                    <div v-for=\"n in userList.last_page\" :key=\"n\">\r\n                      <li\r\n                        v-if=\"\r\n                          userList.current_page - 3 < n &&\r\n                          userList.current_page + 3 > n\r\n                        \"\r\n                        class=\"page-item\"\r\n                        :class=\"userList.current_page == n ? 'active' : ''\"\r\n                      >\r\n                        <a class=\"page-link\" @click=\"userPage(n)\" href=\"#\">{{\r\n                          n\r\n                        }}</a>\r\n                      </li>\r\n                    </div>\r\n                    <li\r\n                      class=\"page-item\"\r\n                      v-if=\"userList.current_page != userList.last_page\"\r\n                      @click=\"userPage(userList.current_page + 1)\"\r\n                    >\r\n                      <a class=\"page-link\">Next</a>\r\n                    </li>\r\n                  </ul>\r\n                </nav>\r\n              </div>\r\n\r\n              <div class=\"col-md-12\" v-if=\"!showing\">\r\n                <div class=\"text-center my-5\">\r\n                  <vue-feather\r\n                    type=\"loader\"\r\n                    size=\"50\"\r\n                    animation=\"spin\"\r\n                  ></vue-feather>\r\n                </div>\r\n              </div>\r\n\r\n              <div\r\n                class=\"col-md-12 my-5 text-center\"\r\n                v-if=\"showing && !userList.data.length\"\r\n              >\r\n                Sorry, username is not found.\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n<script>\r\nimport AdminHeader from \"@/components/Admin/Header\";\r\nimport AdminSidebar from \"@/components/Admin/Sidebar\";\r\nimport VueFeather from \"vue-feather\";\r\nimport moment from \"moment\";\r\n\r\nexport default {\r\n  name: \"Student\",\r\n  components: {\r\n    AdminHeader,\r\n    AdminSidebar,\r\n    VueFeather,\r\n  },\r\n  data() {\r\n    return {\r\n      sidebar: \"sidebar-left\",\r\n      sidebarStatus: true,\r\n      header: \"content\",\r\n      user: [],\r\n      userList: [],\r\n      filter: \"\",\r\n      search: \"\",\r\n      is_verify: false,\r\n      showing: false,\r\n      category: \"\",\r\n    };\r\n  },\r\n  methods: {\r\n    sidebarToggle() {\r\n      if (this.sidebarStatus == true) {\r\n        this.sidebarStatus = false;\r\n        this.header = \"full-content\";\r\n        this.sidebar = \"d-none\";\r\n      } else {\r\n        this.sidebarStatus = true;\r\n        this.sidebar = \"sidebar-left sidebar-mobile\";\r\n        this.header = \"content\";\r\n      }\r\n    },\r\n    reload() {\r\n      this.showing = true;\r\n    },\r\n    userPage(n) {\r\n      this.showing = false;\r\n      if (this.category == \"search\") {\r\n        if (this.filter == \"\") {\r\n          this.filter = \"name\";\r\n        }\r\n        this.$axios\r\n          .post(\r\n            this.$api_url + \"filter/student?page=\" + n,\r\n            {\r\n              category: this.filter,\r\n              value: this.search,\r\n            },\r\n            {\r\n              headers: {\r\n                Authorization: \"Bearer \" + this.user.token,\r\n              },\r\n            }\r\n          )\r\n          .then((res) => {\r\n            if (res.data.success) {\r\n              this.reload();\r\n              this.userList = res.data.data;\r\n            }\r\n          })\r\n          .catch((error) => {\r\n            console.log(error);\r\n          });\r\n      } else {\r\n        this.$axios\r\n          .get(this.$api_url + \"user?page=\" + n, {\r\n            headers: {\r\n              Authorization: \"Bearer \" + this.user.token,\r\n            },\r\n          })\r\n          .then((res) => {\r\n            this.reload();\r\n            this.userList = res.data.data;\r\n          })\r\n          .catch((error) => {\r\n            console.log(error);\r\n          });\r\n      }\r\n    },\r\n    formatDate(n) {\r\n      return moment(n).format(\"LL\");\r\n    },\r\n    verifyProcess() {\r\n      this.showing = false;\r\n      this.$axios\r\n        .post(\r\n          this.$api_url + \"filter/student\",\r\n          {\r\n            category: \"is_verified\",\r\n            value: this.is_verify,\r\n          },\r\n          {\r\n            headers: {\r\n              Authorization: \"Bearer \" + this.user.token,\r\n            },\r\n          }\r\n        )\r\n        .then((res) => {\r\n          if (res.data.success) {\r\n            this.reload();\r\n            this.userList = res.data.data;\r\n          }\r\n        })\r\n        .catch((error) => {\r\n          console.log(error);\r\n        });\r\n    },\r\n    searchProcess() {\r\n      this.showing = false;\r\n      this.category = \"search\";\r\n      if (this.filter == \"\") {\r\n        this.filter = \"name\";\r\n      }\r\n      this.$axios\r\n        .post(\r\n          this.$api_url + \"filter/student\",\r\n          {\r\n            category: this.filter,\r\n            value: this.search,\r\n          },\r\n          {\r\n            headers: {\r\n              Authorization: \"Bearer \" + this.user.token,\r\n            },\r\n          }\r\n        )\r\n        .then((res) => {\r\n          if (res.data.success) {\r\n            this.reload();\r\n            this.userList = res.data.data;\r\n          }\r\n        })\r\n        .catch((error) => {\r\n          console.log(error);\r\n        });\r\n    },\r\n    refreshProcess() {\r\n      this.search = \"\";\r\n      this.filter = \"\";\r\n      this.showing = false;\r\n      this.$axios\r\n        .get(this.$api_url + \"user\", {\r\n          headers: {\r\n            Authorization: \"Bearer \" + this.user.token,\r\n          },\r\n        })\r\n        .then((res) => {\r\n          if (res.data.success) {\r\n            this.reload();\r\n            this.userList = res.data.data;\r\n          }\r\n        })\r\n        .catch((error) => {\r\n          console.log(error);\r\n        });\r\n    },\r\n    getUserData() {\r\n      this.$axios\r\n        .get(this.$api_url + \"user\", {\r\n          headers: {\r\n            Authorization: \"Bearer \" + this.user.token,\r\n          },\r\n        })\r\n        .then((res) => {\r\n          if (res.data.success) {\r\n            this.reload();\r\n            this.userList = res.data.data;\r\n          }\r\n        })\r\n        .catch((error) => {\r\n          console.log(error);\r\n        });\r\n    },\r\n    goToDetail(id) {\r\n      this.$router.push({ path: \"/admin/user/\" + id });\r\n    },\r\n  },\r\n  created() {\r\n    this.user = this.$auth.check();\r\n\r\n    if (this.user) {\r\n      this.getUserData();\r\n    }\r\n  },\r\n};\r\n</script>\r\n<style scoped>\r\n.join-date {\r\n  font-size: 12px;\r\n}\r\n\r\n.card-label {\r\n  font-size: 14px;\r\n  margin: 0 !important;\r\n  line-height: 1.5;\r\n}\r\n\r\n.fade-enter-active {\r\n  transition: opacity 0.5s ease;\r\n}\r\n\r\n.fade-enter-from {\r\n  opacity: 0;\r\n}\r\n\r\n.card-list {\r\n  border: 3px solid rgba(223, 220, 220, 0.933);\r\n  transition: all 0.5s ease;\r\n  cursor: pointer;\r\n}\r\n\r\n.card-list:hover {\r\n  border: 3px solid rgb(177, 163, 255);\r\n}\r\n</style>"],"sourceRoot":""}]}