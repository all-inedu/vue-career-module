plate>\n  <div id=\"dashboard\">\n    <div class=\"container-fluid ps-0\">\n      <div class=\"row g-0\">\n        <div :class=\"sidebar\" class=\"sidebar-admin sticky-top\">\n          <transition name=\"slide\">\n            <AdminSidebar\n              v-if=\"sidebarStatus\"\n              :display=\"sidebarToggle\"\n              menu=\"student\"\n              submenu=\"student\"\n            ></AdminSidebar>\n          </transition>\n        </div>\n        <div :class=\"header\">\n          <AdminHeader :display=\"sidebarToggle\"></AdminHeader>\n          <div class=\"container p-md-3 p-2\">\n            <div class=\"row\">\n              <div class=\"col-md-12\">\n                <h4>Student List</h4>\n              </div>\n\n              <div class=\"col-md-12\">\n                <div class=\"row\">\n                  <div class=\"col-md-5\">\n                    <div class=\"input-group\">\n                      <select class=\"form-select w-25\" v-model=\"filter\">\n                        <option value=\"\">Filter</option>\n                        <option value=\"name\">Name</option>\n                        <option value=\"email\">Email</option>\n                      </select>\n                      <input\n                        v-model=\"search\"\n                        @keyup.enter=\"searchProcess\"\n                        type=\"text\"\n                        class=\"form-control w-75\"\n                        aria-label=\"Text input with dropdown button\"\n                      />\n                    </div>\n                  </div>\n                  <div class=\"col-md-7 text-end\">\n                    <vue-feather\n                      type=\"refresh-cw\"\n                      class=\"me-3 pointer\"\n                    ></vue-feather>\n                    <div class=\"form-check d-inline-block form-switch\">\n                      <label class=\"form-check-label\">Is verified?</label>\n                      <input\n                        class=\"form-check-input\"\n                        type=\"checkbox\"\n                        role=\"switch\"\n                        v-model=\"is_verify\"\n                        @change=\"verifyProcess\"\n                      />\n                    </div>\n                  </div>\n                </div>\n              </div>\n\n              <div class=\"col-md-12 mt-3\">\n                <div class=\"row row-cols-3\">\n                  <div\n                    class=\"col mb-md-4 mb-2\"\n                    v-for=\"item in userList.data\"\n                    :key=\"item.id\"\n                  >\n                    <div class=\"card h-100 shadow\">\n                      <div class=\"card-body\">\n                        <div class=\"row align-items-center\">\n                          <div class=\"col-3 text-center\">\n                            <vue-feather type=\"user\" size=\"50\"></vue-feather>\n                          </div>\n                          <div class=\"col-9\">\n                            <h6 class=\"my-0 mb-2\">\n                              <b class=\"text-uppercase\">\n                                {{ item.first_name }} {{ item.last_name }}\n                                <vue-feather\n                                  v-if=\"item.is_verified == 1\"\n                                  type=\"check\"\n                                  stroke=\"green\"\n                                ></vue-feather>\n                              </b>\n                            </h6>\n                            <p class=\"card-label text-primary\">\n                              {{ item.email }}\n                            </p>\n                            <p class=\"card-label\">\n                              {{ item.phone_number }} |\n                              {{ formatDate(item.birthday) }}\n                            </p>\n                          </div>\n                        </div>\n                      </div>\n                      <div class=\"card-footer\">\n                        <div class=\"float-start\">\n                          <label class=\"join-date\">\n                            <vue-feather\n                              type=\"calendar\"\n                              size=\"20\"\n                              class=\"ms-2\"\n                            ></vue-feather>\n                            Join date on {{ formatDate(item.created_at) }}\n                          </label>\n                        </div>\n                        <div class=\"float-end\">\n                          <vue-feather\n                            type=\"arrow-right\"\n                            size=\"20\"\n                            class=\"ms-2\"\n                          ></vue-feather>\n                        </div>\n                      </div>\n                    </div>\n                  </div>\n                </div>\n              </div>\n\n              <div class=\"col-md-12 mt-3\">\n                <nav aria-label=\"Page navigation example\">\n                  <ul class=\"pagination justify-content-center\">\n                    <li\n                      class=\"page-item\"\n                      v-if=\"userList.current_page != 1\"\n                      @click=\"userPage(userList.current_page - 1)\"\n                    >\n                      <a class=\"page-link\">Previous</a>\n                    </li>\n                    <div v-for=\"n in userList.last_page\" :key=\"n\">\n                      <li class=\"page-item\">\n                        <a class=\"page-link\" @click=\"userPage(n)\" href=\"#\">{{\n                          n\n                        }}</a>\n                      </li>\n                    </div>\n                    <li\n                      class=\"page-item\"\n                      v-if=\"userList.current_page != userList.last_page\"\n                      @click=\"userPage(userList.current_page + 1)\"\n                    >\n                      <a class=\"page-link\">Next</a>\n                    </li>\n                  </ul>\n                </nav>\n              </div>\n            </div>\n          </div>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n<script>\nimport AdminHeader from \"@/components/Admin/Header\";\nimport AdminSidebar from \"@/components/Admin/Sidebar\";\nimport VueFeather from \"vue-feather\";\nimport axios from \"axios\";\nimport moment from \"moment\";\n\nexport default {\n  name: \"Student\",\n  components: {\n    AdminHeader,\n    AdminSidebar,\n    VueFeather,\n  },\n  data() {\n    return {\n      api_url: \"https://api-cm.all-inedu.com/api/v1/\",\n      sidebar: \"col-md-3\",\n      sidebarStatus: true,\n      header: \"col-md-9\",\n      userSession: [],\n      userList: [],\n      filter: \"\",\n      search: \"\",\n      is_verify: false,\n    };\n  },\n  methods: {\n    sidebarToggle() {\n      if (this.sidebarStatus == true) {\n        this.sidebarStatus = false;\n        this.sidebar = \"d-none\";\n        this.header = \"col-md-12\";\n      } else {\n        this.sidebarStatus = true;\n        this.sidebar = \"col-md-3\";\n        this.header = \"col-md-9\";\n      }\n    },\n    userPage(n) {\n      axios\n        .get(this.api_url + \"user?page=\" + n, {\n          headers: {\n            Authorization: \"Bearer \" + this.userSession.data.token,\n          },\n        })\n        .then((res) => {\n          this.userList = res.data.data;\n        })\n        .catch((error) => {\n          console.log(error);\n        });\n    },\n    formatDate(n) {\n      return moment(n).format(\"LL\");\n    },\n    verifyProcess() {\n      axios\n        .post(\n          this.api_url + \"filter/student\",\n          {\n            category: \"is_verified\",\n            value: this.is_verify,\n          },\n          {\n            headers: {\n              Authorization: \"Bearer \" + this.userSession.data.token,\n            },\n          }\n        )\n        .then((res) => {\n          if (res.data.success) {\n            this.userList = res.data.data;\n          }\n        })\n        .catch((error) => {\n          console.log(error);\n        });\n    },\n    searchProcess() {\n      axios\n        .post(\n          this.api_url + \"filter/student\",\n          {\n            category: this.filter,\n            value: this.search,\n          },\n          {\n            headers: {\n              Authorization: \"Bearer \" + this.userSession.data.token,\n            },\n          }\n        )\n        .then((res) => {\n          if (res.data.success) {\n            this.userList = res.data.data;\n          }\n        })\n        .catch((error) => {\n          console.log(error);\n        });\n    },\n  },\n  created() {\n    if (sessionStorage.getItem(\"user\") != null) {\n      this.userSession = JSON.parse(sessionStorage.getItem(\"user\"));\n\n      axios\n        .get(this.api_url + \"user\", {\n          headers: {\n            Authorization: \"Bearer \" + this.userSession.data.token,\n          },\n        })\n        .then((res) => {\n          if (res.data.success) {\n            this.userList = res.data.data;\n          }\n        })\n        .catch((error) => {\n          console.log(error);\n        });\n    }\n  },\n};\n</script>\n<style scoped>\n.join-date {\n  font-size: 12px;\n}\n\n.card-label {\n  font-size: 14px;\n  margin: 0 !important;\n  line-height: 1.5;\n}\n</style>"],"sourceRoot":""}]}