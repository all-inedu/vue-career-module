AA,OAAO,EAAE;AACP,UAAA,aAAa,EAAE,YAAY,KAAK,IAAL,CAAU;AAD9B;AADkC,OAD/C,EAMG,IANH,CAMQ,UAAC,QAAD,EAAc;AAClB,QAAA,KAAI,CAAC,YAAL,GAAoB,QAAQ,CAAC,IAAT,CAAc,OAAlC;;AACA,QAAA,KAAI,CAAC,KAAL,CAAW,cAAX,EAA2B,QAAQ,CAAC,IAAT,CAAc,OAAzC;;AACA,QAAA,KAAI,CAAC,WAAL;AACD,OAVH,EAWG,KAXH,CAWS,UAAC,KAAD,EAAW;AAChB,QAAA,OAAO,CAAC,GAAR,CAAY,KAAZ;AACD,OAbH;AAcD,KApBM;AAqBP,IAAA,WArBO,yBAqBO;AAAA;;AACZ,WAAK,IAAL,GAAY,IAAZ;AACA,WAAK,MAAL,CACG,GADH,CACO,KAAK,QAAL,GAAgB,sBAAhB,GAAyC,KAAK,YAAL,CAAkB,EADlE,EACsE;AAClE,QAAA,OAAO,EAAE;AACP,UAAA,aAAa,EAAE,YAAY,KAAK,IAAL,CAAU;AAD9B;AADyD,OADtE,EAMG,IANH,CAMQ,UAAC,QAAD,EAAc;AAClB;AACA,YAAI,QAAQ,CAAC,IAAT,CAAc,IAAd,CAAmB,IAAnB,IAA2B,IAA/B,EAAqC;AACnC,UAAA,MAAI,CAAC,IAAL,GAAY,KAAZ;AACA,UAAA,MAAI,CAAC,SAAL,GAAiB,QAAQ,CAAC,IAAT,CAAc,IAAd,CAAmB,IAApC;AACF,SAHA,MAGO;AACL,UAAA,MAAI,CAAC,OAAL,CAAa,IAAb,CAAkB;AAAE,YAAA,IAAI,EAAE;AAAR,WAAlB;;AACA,UAAA,MAAI,CAAC,MAAL,CAAY,KAAZ,CAAkB,SAAlB,EAA6B,yBAA7B;AACF;AACD,OAfH,EAgBG,KAhBH,CAgBS,UAAC,KAAD,EAAW;AAChB,QAAA,OAAO,CAAC,GAAR,CAAY,KAAZ;AACD,OAlBH;AAmBD,KA1CM;AA2CP,IAAA,WA3CO,uBA2CK,IA3CL,EA2CW;AAChB;AACA,WAAK,IAAL,GAAY,KAAZ;AACA,WAAK,YAAL,GAAoB,IAApB;AACD,KA/CM;AAgDP,IAAA,YAhDO,wBAgDM,IAhDN,EAgDY;AACjB,WAAK,IAAL,GAAY,IAAZ;AACA,WAAK,SAAL,GAAiB,EAAjB;AACA,WAAK,cAAL,CAAoB,IAApB;AACD,KApDM;AAqDP,IAAA,YArDO,wBAqDM,IArDN,EAqDY;AACjB,UAAI,IAAJ,EAAU;AACR,aAAK,IAAL,GAAY,IAAZ;AACA,aAAK,WAAL;AACF;AACD,KA1DM;AA2DP,IAAA,aA3DO,yBA2DO,IA3DP,EA2Da;AAClB,UAAI,IAAJ,EAAU;AACR,aAAK,KAAL,CAAW,UAAX,EAAuB,IAAvB,EADQ,CAER;AACF;AACD;AAhEM,GAvBI;AAyFb,EAAA,YAzFa,0BAyFE;AACb,SAAK,OAAL;AACA,SAAK,WAAL;AACD,GA5FY;AA6Fb,EAAA,OA7Fa,qBA6FH;AACR,SAAK,IAAL,GAAY,KAAK,KAAL,CAAW,KAAX,EAAZ;AACA,SAAK,OAAL;AACA,SAAK,WAAL;AACD;AAjGY,CAAf","sourcesContent":["<template>\n  <div id=\"Part\">\n    <!-- LIST PART  -->\n    <transition name=\"fade\">\n      <div class=\"row\" v-if=\"part\">\n        <div class=\"col-md-12 mt-md-0 mt-3\">\n          <div class=\"card\">\n            <div class=\"card-body\">\n              <h4>\n                {{ outline_data.outline_name || outline_data.name }}\n              </h4>\n              <hr class=\"my-2\" />\n              <div\n                class=\"row row-cols-md-4 row-cols-1 align-items-stretch mt-3\"\n                v-if=\"!load\"\n              >\n                <div\n                  class=\"col mb-3\"\n                  v-for=\"(i, index) in part_data\"\n                  :key=\"index\"\n                >\n                  <div\n                    class=\"card shadow h-100 card-part\"\n                    @click=\"goToElement(i)\"\n                  >\n                    <span class=\"card-number\">\n                      {{ index + 1 }}\n                    </span>\n                    <div class=\"card-body item\">\n                      {{ i.part_title }}\n                    </div>\n                    <div class=\"px-2 pb-2\" v-if=\"i.percentage > 0\">\n                      <div class=\"progress\">\n                        <div\n                          :class=\"\n                            i.percentage + '%' >= '100%'\n                              ? 'progress-bar bg-complete'\n                              : ' progress-bar bg-ongoing'\n                          \"\n                          role=\"progressbar\"\n                          :style=\"{ width: i.percentage + '%' }\"\n                          aria-valuenow=\"25\"\n                          aria-valuemin=\"0\"\n                          aria-valuemax=\"100\"\n                        >\n                          {{ i.percentage }} %\n                        </div>\n                      </div>\n                    </div>\n                  </div>\n                </div>\n              </div>\n              <div v-if=\"load\">\n                <div class=\"text-center my-5\">\n                  <vue-feather\n                    type=\"loader\"\n                    size=\"50\"\n                    animation=\"spin\"\n                  ></vue-feather>\n                </div>\n              </div>\n            </div>\n          </div>\n        </div>\n      </div>\n    </transition>\n\n    <!-- ELEMENT  -->\n    <transition name=\"fade\">\n      <div class=\"row\" v-if=\"!part\">\n        <div class=\"col-md-12 mt-md-0 mt-3\">\n          <v-element\n            :part=\"part_default\"\n            :module_id=\"module_id\"\n            @part-element=\"checkElement\"\n            @outline=\"checkOutline\"\n            @progress=\"checkProgress\"\n          ></v-element>\n        </div>\n      </div>\n    </transition>\n  </div>\n</template>\n\n<script>\n// @ is an alias to /src\nimport hElement from \"@/components/Home/ModuleDetail/Element\";\nimport VueFeather from \"vue-feather\";\n\nexport default {\n  name: \"moduleBanner\",\n  props: {\n    outline: Array,\n    module_id: String,\n    part_show: Boolean,\n  },\n  components: {\n    \"v-element\": hElement,\n    VueFeather,\n  },\n  data() {\n    return {\n      user: [],\n      load: true,\n      outline_data: [],\n      part: true,\n      part_data: [],\n      part_default: [],\n      percent: \"100%\",\n      data: [],\n    };\n  },\n  methods: {\n    getProp() {\n      this.outline_data = this.outline;\n      this.part = this.part_show;\n    },\n    getOutlineData(id) {\n      this.$axios\n        .get(this.$api_url + \"detail/outline/\" + id, {\n          headers: {\n            Authorization: \"Bearer \" + this.user.token,\n          },\n        })\n        .then((response) => {\n          this.outline_data = response.data.outline;\n          this.$emit(\"outline-data\", response.data.outline);\n          this.getPartData();\n        })\n        .catch((error) => {\n          console.log(error);\n        });\n    },\n    getPartData() {\n      this.load = true;\n      this.$axios\n        .get(this.$api_url + \"listen/part/outline/\" + this.outline_data.id, {\n          headers: {\n            Authorization: \"Bearer \" + this.user.token,\n          },\n        })\n        .then((response) => {\n          // console.log(response.data);\n          if (response.data.data.part != null) {\n            this.load = false;\n            this.part_data = response.data.data.part;\n          } else {\n            this.$router.push({ path: \"/\" });\n            this.$alert.toast(\"warning\", \"The module is not found\");\n          }\n        })\n        .catch((error) => {\n          console.log(error);\n        });\n    },\n    goToElement(data) {\n      // console.log(data);\n      this.part = false;\n      this.part_default = data;\n    },\n    checkOutline(data) {\n      this.part = true;\n      this.part_data = [];\n      this.getOutlineData(data);\n    },\n    checkElement(data) {\n      if (data) {\n        this.part = true;\n        this.getPartData();\n      }\n    },\n    checkProgress(data) {\n      if (data) {\n        this.$emit(\"progress\", data);\n        // console.log(data);\n      }\n    },\n  },\n  beforeUpdate() {\n    this.getProp();\n    this.getPartData();\n  },\n  created() {\n    this.user = this.$auth.check();\n    this.getProp();\n    this.getPartData();\n  },\n};\n</script>\n\n<style scoped>\n.item {\n  display: flex;\n  align-items: center;\n  text-align: center;\n  padding: 40px 10px;\n}\n\n.card-part {\n  background: hsla(226, 52%, 29%, 1);\n\n  background: linear-gradient(\n    135deg,\n    hsla(226, 52%, 29%, 1) 0%,\n    hsla(226, 39%, 49%, 1) 100%\n  );\n\n  background: -moz-linear-gradient(\n    135deg,\n    hsla(226, 52%, 29%, 1) 0%,\n    hsla(226, 39%, 49%, 1) 100%\n  );\n\n  background: -webkit-linear-gradient(\n    135deg,\n    hsla(226, 52%, 29%, 1) 0%,\n    hsla(226, 39%, 49%, 1) 100%\n  );\n\n  color: white;\n  font-weight: bold;\n  border: 4px solid rgba(255, 255, 255, 0.014);\n  transition: all 0.3s ease;\n  cursor: pointer;\n}\n\n.card-part:hover {\n  border-left: 4px solid rgb(218, 139, 49);\n}\n\n.card-number {\n  position: absolute;\n  top: -15px;\n  right: 10px;\n  font-size: 4em;\n  font-weight: 900;\n  color: #dedede3b;\n}\n\n.fade-enter-active {\n  transition: opacity 0.5s ease;\n}\n\n.fade-enter-from {\n  opacity: 0;\n}\n\n.bg-complete {\n  background: rgb(58, 233, 58);\n  color: #000;\n}\n\n.bg-ongoing {\n  background: rgb(72, 104, 247);\n}\n</style>"],"sourceRoot":""}]}