K,CADqB;;;;AAAA,GAAvB,C,EAgBA,YAAA,CAMa,WANb,EAMa;AAND,IAAA,IAAI,EAAC;AAMJ,GANb,EAAuB;sBACrB;AAAA,aAIW,CAFH,KAAA,CAAA,OAAA,IAAW,KAAA,CAAA,OAAA,IAAO,C,kBAF1B,YAAA,CAIW,mBAJX,EAIW;cAAA;AAHR,QAAA,cAAa,EAAE,QAAA,CAAA;AAGP,OAJX,E,IAAA,E;;AAAA,Q,kBAAA,C,qCAIW,CAJX;AAAA,K,CADqB;;;;AAAA,GAAvB,C,EAQA,YAAA,CAKa,WALb,EAKa;AALD,IAAA,IAAI,EAAC;AAKJ,GALb,EAAuB;sBACrB;AAAA,aAGa,CADL,KAAA,CAAA,OAAA,IAAW,KAAA,CAAA,OAAA,IAAO,C,kBAF1B,YAAA,CAGa,oBAHb,EAGa;cAAA;AAFV,QAAA,cAAa,EAAE,QAAA,CAAA;AAEL,OAHb,E,IAAA,E;;AAAA,Q,kBAAA,C,qCAGa,CAHb;AAAA,K,CADqB;;;;AAAA,GAAvB,C,EAOA,YAAA,CAKa,WALb,EAKa;AALD,IAAA,IAAI,EAAC;AAKJ,GALb,EAAuB;sBACrB;AAAA,aAGU,CADF,KAAA,CAAA,OAAA,IAAW,KAAA,CAAA,OAAA,IAAO,C,kBAF1B,YAAA,CAGU,iBAHV,EAGU;cAAA;AAFP,QAAA,cAAa,EAAE,QAAA,CAAA;AAER,OAHV,E,IAAA,E;;AAAA,Q,kBAAA,C,qCAGU,CAHV;AAAA,K,CADqB;;;;AAAA,GAAvB,C,EAOA,YAAA,CAKa,WALb,EAKa;AALD,IAAA,IAAI,EAAC;AAKJ,GALb,EAAuB;sBACrB;AAAA,aAGa,CADL,KAAA,CAAA,OAAA,IAAW,KAAA,CAAA,OAAA,IAAO,C,kBAF1B,YAAA,CAGa,oBAHb,EAGa;cAAA;AAFV,QAAA,cAAa,EAAE,QAAA,CAAA;AAEL,OAHb,E,IAAA,E;;AAAA,Q,kBAAA,C,qCAGa,CAHb;AAAA,K,CADqB;;;;AAAA,GAAvB,C,CAxDF,E;;AAAA,GA+DI,CAzEN,CA0EI,CA3EN,CA4EI,CA9EN,C","sourcesContent":["<template>\n  <div id=\"module\">\n    <AdminCheck></AdminCheck>\n    <div class=\"container-fluid ps-0\">\n      <div class=\"row g-0\">\n        <transition name=\"slide\">\n          <div\n            :class=\"sidebar\"\n            class=\"sidebar-admin sticky-top\"\n            v-if=\"sidebarStatus\"\n          >\n            <AdminSidebar :display=\"sidebarToggle\" menu=\"module\"></AdminSidebar>\n          </div>\n        </transition>\n        <div :class=\"header\">\n          <AdminHeader :display=\"sidebarToggle\"></AdminHeader>\n          <transition name=\"fade\">\n            <div class=\"container\" v-if=\"!showing\">\n              <div class=\"row\">\n                <div class=\"col-md-12\">\n                  <div class=\"text-center my-5\">\n                    <vue-feather\n                      type=\"loader\"\n                      size=\"50\"\n                      animation=\"spin\"\n                    ></vue-feather>\n                  </div>\n                </div>\n              </div>\n            </div>\n          </transition>\n          <!-- Stepper  -->\n          <transition name=\"fade\">\n            <div class=\"container sticky-top bg-white\" v-if=\"showing\">\n              <div class=\"row justify-content-center\">\n                <div class=\"col-md-8\">\n                  <ul class=\"Container-progessbar\">\n                    <li :class=\"section >= 1 ? 'active' : ''\">Module</li>\n                    <li :class=\"section >= 2 ? 'active' : ''\">Outline</li>\n                    <li :class=\"section >= 3 ? 'active' : ''\">Part</li>\n                    <li :class=\"section >= 4 ? 'active' : ''\">Element</li>\n                    <li :class=\"section >= 5 ? 'active' : ''\">Complete</li>\n                  </ul>\n                </div>\n              </div>\n            </div>\n          </transition>\n\n          <transition name=\"fade\">\n            <v-module\n              @check-section=\"checkSection\"\n              v-if=\"showing && section == 1\"\n            >\n            </v-module>\n          </transition>\n\n          <transition name=\"fade\">\n            <v-outline\n              @check-section=\"checkSection\"\n              v-if=\"showing && section == 2\"\n            ></v-outline>\n          </transition>\n\n          <transition name=\"fade\">\n            <v-part\n              @check-section=\"checkSection\"\n              v-if=\"showing && section == 3\"\n            ></v-part>\n          </transition>\n\n          <transition name=\"fade\">\n            <v-element\n              @check-section=\"checkSection\"\n              v-if=\"showing && section == 4\"\n            ></v-element>\n          </transition>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n<script>\nimport AdminCheck from \"@/components/Admin/UserCheck\";\nimport AdminHeader from \"@/components/Admin/Header\";\nimport AdminSidebar from \"@/components/Admin/Sidebar\";\n\nimport Module from \"@/components/Admin/Module/Module\";\nimport Outline from \"@/components/Admin/Module/Outline\";\nimport Part from \"@/components/Admin/Module/Part\";\nimport Element from \"@/components/Admin/Module/Element\";\n\nimport VueFeather from \"vue-feather\";\nimport Swal from \"sweetalert2\";\n\nimport \"@/assets/css/stepper.css\";\n\nexport default {\n  name: \"Module Create\",\n  components: {\n    AdminCheck,\n    AdminHeader,\n    AdminSidebar,\n    VueFeather,\n    \"v-module\": Module,\n    \"v-outline\": Outline,\n    \"v-part\": Part,\n    \"v-element\": Element,\n  },\n  data() {\n    return {\n      api_url: \"https://api-cm.all-inedu.com/api/v1/\",\n      userSession: [],\n      sidebar: \"sidebar-left\",\n      sidebarStatus: true,\n      header: \"content\",\n      showing: false,\n      section: 1,\n    };\n  },\n  methods: {\n    toast(status, title) {\n      const Toast = Swal.mixin({\n        toast: true,\n        width: \"32rem\",\n        position: \"top-end\",\n        showConfirmButton: false,\n        timer: 3000,\n        timerProgressBar: true,\n        didOpen: (toast) => {\n          toast.addEventListener(\"mouseenter\", Swal.stopTimer);\n          toast.addEventListener(\"mouseleave\", Swal.resumeTimer);\n        },\n      });\n\n      Toast.fire({\n        icon: status,\n        title: title,\n      });\n    },\n    sidebarToggle() {\n      if (this.sidebarStatus == true) {\n        this.sidebarStatus = false;\n        this.header = \"full-content\";\n        this.sidebar = \"d-none\";\n      } else {\n        this.sidebarStatus = true;\n        this.sidebar = \"sidebar-left sidebar-mobile\";\n        this.header = \"content\";\n      }\n    },\n    reload() {\n      this.showing = true;\n    },\n    checkSection(data) {\n      this.section = data;\n    },\n  },\n  created() {\n    setTimeout(() => {\n      this.reload();\n    }, 1000);\n  },\n};\n</script>\n<style scoped>\n.fade-enter-active {\n  transition: opacity 0.5s ease;\n}\n\n.fade-enter-from {\n  opacity: 0;\n}\n</style>"],"sourceRoot":""}]}