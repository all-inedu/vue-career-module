,OAAO,CAAC,GAAR,CAAY,KAAZ;AACD,OArBH;AAsBD,KAtFM;AAuFP,IAAA,aAvFO,2BAuFS;AAAA;;AACd,WAAK,OAAL,GAAe,KAAf;AACA,WAAK,QAAL,GAAgB,QAAhB;;AACA,UAAI,KAAK,MAAL,IAAe,EAAnB,EAAuB;AACrB,aAAK,MAAL,GAAc,MAAd;AACF;;AACA,MAAA,KAAI,CACD,IADH,CAEI,KAAK,OAAL,GAAe,gBAFnB,EAGI;AACE,QAAA,QAAQ,EAAE,KAAK,MADjB;AAEE,QAAA,KAAK,EAAE,KAAK;AAFd,OAHJ,EAOI;AACE,QAAA,OAAO,EAAE;AACP,UAAA,aAAa,EAAE,YAAY,KAAK,WAAL,CAAiB,IAAjB,CAAsB;AAD1C;AADX,OAPJ,EAaG,IAbH,CAaQ,UAAC,GAAD,EAAS;AACb,YAAI,GAAG,CAAC,IAAJ,CAAS,OAAb,EAAsB;AACpB,UAAA,MAAI,CAAC,MAAL;;AACA,UAAA,MAAI,CAAC,QAAL,GAAgB,GAAG,CAAC,IAAJ,CAAS,IAAzB;AACF;AACD,OAlBH,EAmBG,KAnBH,CAmBS,UAAC,KAAD,EAAW;AAChB,QAAA,OAAO,CAAC,GAAR,CAAY,KAAZ;AACD,OArBH;AAsBD,KAnHM;AAoHP,IAAA,cApHO,4BAoHU;AAAA;;AACf,WAAK,MAAL,GAAc,EAAd;AACA,WAAK,MAAL,GAAc,EAAd;AACA,WAAK,OAAL,GAAe,KAAf;AACA,MAAA,KAAI,CACD,GADH,CACO,KAAK,OAAL,GAAe,MADtB,EAC8B;AAC1B,QAAA,OAAO,EAAE;AACP,UAAA,aAAa,EAAE,YAAY,KAAK,WAAL,CAAiB,IAAjB,CAAsB;AAD1C;AADiB,OAD9B,EAMG,IANH,CAMQ,UAAC,GAAD,EAAS;AACb,YAAI,GAAG,CAAC,IAAJ,CAAS,OAAb,EAAsB;AACpB,UAAA,MAAI,CAAC,MAAL;;AACA,UAAA,MAAI,CAAC,QAAL,GAAgB,GAAG,CAAC,IAAJ,CAAS,IAAzB;AACF;AACD,OAXH,EAYG,KAZH,CAYS,UAAC,KAAD,EAAW;AAChB,QAAA,OAAO,CAAC,GAAR,CAAY,KAAZ;AACD,OAdH;AAeD,KAvIM;AAwIP,IAAA,UAxIO,sBAwII,EAxIJ,EAwIQ;AACb,WAAK,OAAL,CAAa,IAAb,CAAkB;AAAE,QAAA,IAAI,EAAE,iBAAiB;AAAzB,OAAlB;AACD;AA1IM,GAvBI;AAmKb,EAAA,OAnKa,qBAmKH;AAAA;;AACR,QAAI,cAAc,CAAC,OAAf,CAAuB,MAAvB,KAAkC,IAAtC,EAA4C;AAC1C,WAAK,WAAL,GAAmB,IAAI,CAAC,KAAL,CAAW,cAAc,CAAC,OAAf,CAAuB,MAAvB,CAAX,CAAnB;AAEA,MAAA,KAAI,CACD,GADH,CACO,KAAK,OAAL,GAAe,MADtB,EAC8B;AAC1B,QAAA,OAAO,EAAE;AACP,UAAA,aAAa,EAAE,YAAY,KAAK,WAAL,CAAiB,IAAjB,CAAsB;AAD1C;AADiB,OAD9B,EAMG,IANH,CAMQ,UAAC,GAAD,EAAS;AACb,YAAI,GAAG,CAAC,IAAJ,CAAS,OAAb,EAAsB;AACpB,UAAA,MAAI,CAAC,MAAL;;AACA,UAAA,MAAI,CAAC,QAAL,GAAgB,GAAG,CAAC,IAAJ,CAAS,IAAzB;AACF;AACD,OAXH,EAYG,KAZH,CAYS,UAAC,KAAD,EAAW;AAChB,QAAA,OAAO,CAAC,GAAR,CAAY,KAAZ;AACD,OAdH;AAeF;AACD;AAvLY,CAAf","sourcesContent":["<template>\n  <div id=\"dashboard\">\n    <AdminCheck></AdminCheck>\n    <div class=\"container-fluid ps-0\">\n      <div class=\"row g-0\">\n        <div :class=\"sidebar\" class=\"sidebar-admin sticky-top\">\n          <transition name=\"slide\">\n            <AdminSidebar\n              v-if=\"sidebarStatus\"\n              :display=\"sidebarToggle\"\n              menu=\"user\"\n              submenu=\"user\"\n            ></AdminSidebar>\n          </transition>\n        </div>\n        <div :class=\"header\">\n          <AdminHeader :display=\"sidebarToggle\"></AdminHeader>\n          <div class=\"container p-md-3 p-2\">\n            <div class=\"row\">\n              <div class=\"col-md-12\"></div>\n\n              <div class=\"col-md-12\">\n                <div class=\"row\">\n                  <div class=\"col-md-5\">\n                    <div class=\"input-group\">\n                      <select class=\"form-select w-25\" v-model=\"filter\">\n                        <option value=\"\">Filter</option>\n                        <option value=\"name\">Name</option>\n                        <option value=\"email\">Email</option>\n                      </select>\n                      <input\n                        v-model=\"search\"\n                        @keyup.enter=\"searchProcess\"\n                        type=\"text\"\n                        class=\"form-control w-75\"\n                        aria-label=\"Text input with dropdown button\"\n                      />\n                    </div>\n                  </div>\n                  <div class=\"col-md-7 text-end\">\n                    <vue-feather\n                      @click=\"refreshProcess\"\n                      type=\"refresh-cw\"\n                      class=\"me-3 pointer\"\n                    ></vue-feather>\n                    <div class=\"form-check d-inline-block form-switch\">\n                      <label class=\"form-check-label\">Is verified?</label>\n                      <input\n                        class=\"form-check-input\"\n                        type=\"checkbox\"\n                        role=\"switch\"\n                        v-model=\"is_verify\"\n                        @change=\"verifyProcess\"\n                      />\n                    </div>\n                  </div>\n                </div>\n                <hr class=\"my-2\" />\n              </div>\n\n              <div class=\"col-md-12 mt-1\">\n                <transition name=\"fade\">\n                  <div class=\"row row-cols-md-3\" v-if=\"showing\">\n                    <div\n                      class=\"col-12 mb-md-4 mb-2\"\n                      v-for=\"item in userList.data\"\n                      :key=\"item.id\"\n                    >\n                      <div\n                        class=\"card h-100 card-list\"\n                        @click=\"goToDetail(item.id)\"\n                      >\n                        <div class=\"card-body\">\n                          <div class=\"row align-items-center\">\n                            <div class=\"col-3 text-center\">\n                              <vue-feather type=\"user\" size=\"50\"></vue-feather>\n                            </div>\n                            <div class=\"col-9\">\n                              <h6 class=\"my-0 mb-2\">\n                                <b class=\"text-uppercase\">\n                                  {{ item.first_name }} {{ item.last_name }}\n                                  <vue-feather\n                                    v-if=\"item.is_verified == 1\"\n                                    type=\"check\"\n                                    stroke=\"green\"\n                                  ></vue-feather>\n                                </b>\n                              </h6>\n                              <p class=\"card-label text-primary\">\n                                {{ item.email }}\n                              </p>\n                              <p class=\"card-label\">\n                                {{ item.phone_number }} |\n                                {{ formatDate(item.birthday) }}\n                              </p>\n                            </div>\n                          </div>\n                        </div>\n                        <div class=\"card-footer\">\n                          <div class=\"float-start\">\n                            <label class=\"join-date\">\n                              <vue-feather\n                                type=\"calendar\"\n                                size=\"20\"\n                                class=\"ms-2\"\n                              ></vue-feather>\n                              Join date on {{ formatDate(item.created_at) }}\n                            </label>\n                          </div>\n                          <div class=\"float-end\">\n                            <vue-feather\n                              type=\"arrow-right\"\n                              size=\"20\"\n                              class=\"ms-2\"\n                            ></vue-feather>\n                          </div>\n                        </div>\n                      </div>\n                    </div>\n                  </div>\n                </transition>\n              </div>\n\n              <div\n                class=\"col-md-12 mt-3\"\n                v-if=\"\n                  showing && userList.data.length > 0 && userList.last_page > 1\n                \"\n              >\n                <nav aria-label=\"Page navigation example\">\n                  <ul class=\"pagination justify-content-center\">\n                    <li\n                      class=\"page-item\"\n                      v-if=\"userList.current_page != 1\"\n                      @click=\"userPage(userList.current_page - 1)\"\n                    >\n                      <a class=\"page-link\">Previous</a>\n                    </li>\n                    <div v-for=\"n in userList.last_page\" :key=\"n\">\n                      <li\n                        v-if=\"\n                          userList.current_page - 3 < n &&\n                          userList.current_page + 3 > n\n                        \"\n                        class=\"page-item\"\n                        :class=\"userList.current_page == n ? 'active' : ''\"\n                      >\n                        <a class=\"page-link\" @click=\"userPage(n)\" href=\"#\">{{\n                          n\n                        }}</a>\n                      </li>\n                    </div>\n                    <li\n                      class=\"page-item\"\n                      v-if=\"userList.current_page != userList.last_page\"\n                      @click=\"userPage(userList.current_page + 1)\"\n                    >\n                      <a class=\"page-link\">Next</a>\n                    </li>\n                  </ul>\n                </nav>\n                <div class=\"text-center\">\n                  <small>Total : {{ userList.total }} Students</small>\n                </div>\n              </div>\n\n              <div class=\"col-md-12\" v-if=\"!showing\">\n                <div class=\"text-center my-5\">\n                  <vue-feather\n                    type=\"loader\"\n                    size=\"50\"\n                    animation=\"spin\"\n                  ></vue-feather>\n                </div>\n              </div>\n\n              <div\n                class=\"col-md-12 my-5 text-center\"\n                v-if=\"showing && !userList.data.length\"\n              >\n                <h5>Sorry, data is not found.</h5>\n              </div>\n            </div>\n          </div>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n<script>\nimport AdminCheck from \"@/components/Admin/UserCheck\";\nimport AdminHeader from \"@/components/Admin/Header\";\nimport AdminSidebar from \"@/components/Admin/Sidebar\";\nimport VueFeather from \"vue-feather\";\nimport axios from \"axios\";\nimport moment from \"moment\";\n\nexport default {\n  name: \"Student\",\n  components: {\n    AdminCheck,\n    AdminHeader,\n    AdminSidebar,\n    VueFeather,\n  },\n  data() {\n    return {\n      api_url: \"https://api-cm.all-inedu.com/api/v1/\",\n      sidebar: \"col-md-3\",\n      sidebarStatus: true,\n      header: \"col-md-9\",\n      userSession: [],\n      userList: [],\n      filter: \"\",\n      search: \"\",\n      is_verify: false,\n      showing: false,\n      category: \"\",\n    };\n  },\n  methods: {\n    sidebarToggle() {\n      if (this.sidebarStatus == true) {\n        this.sidebarStatus = false;\n        this.sidebar = \"d-none\";\n        this.header = \"col-md-12\";\n      } else {\n        this.sidebarStatus = true;\n        this.sidebar = \"col-md-3\";\n        this.header = \"col-md-9\";\n      }\n    },\n    reload() {\n      this.showing = true;\n    },\n    userPage(n) {\n      this.showing = false;\n      if (this.category == \"search\") {\n        if (this.filter == \"\") {\n          this.filter = \"name\";\n        }\n        axios\n          .post(\n            this.api_url + \"filter/student?page=\" + n,\n            {\n              category: this.filter,\n              value: this.search,\n            },\n            {\n              headers: {\n                Authorization: \"Bearer \" + this.userSession.data.token,\n              },\n            }\n          )\n          .then((res) => {\n            if (res.data.success) {\n              this.reload();\n              this.userList = res.data.data;\n            }\n          })\n          .catch((error) => {\n            console.log(error);\n          });\n      } else {\n        axios\n          .get(this.api_url + \"user?page=\" + n, {\n            headers: {\n              Authorization: \"Bearer \" + this.userSession.data.token,\n            },\n          })\n          .then((res) => {\n            this.reload();\n            this.userList = res.data.data;\n          })\n          .catch((error) => {\n            console.log(error);\n          });\n      }\n    },\n    formatDate(n) {\n      return moment(n).format(\"LL\");\n    },\n    verifyProcess() {\n      this.showing = false;\n      axios\n        .post(\n          this.api_url + \"filter/student\",\n          {\n            category: \"is_verified\",\n            value: this.is_verify,\n          },\n          {\n            headers: {\n              Authorization: \"Bearer \" + this.userSession.data.token,\n            },\n          }\n        )\n        .then((res) => {\n          if (res.data.success) {\n            this.reload();\n            this.userList = res.data.data;\n          }\n        })\n        .catch((error) => {\n          console.log(error);\n        });\n    },\n    searchProcess() {\n      this.showing = false;\n      this.category = \"search\";\n      if (this.filter == \"\") {\n        this.filter = \"name\";\n      }\n      axios\n        .post(\n          this.api_url + \"filter/student\",\n          {\n            category: this.filter,\n            value: this.search,\n          },\n          {\n            headers: {\n              Authorization: \"Bearer \" + this.userSession.data.token,\n            },\n          }\n        )\n        .then((res) => {\n          if (res.data.success) {\n            this.reload();\n            this.userList = res.data.data;\n          }\n        })\n        .catch((error) => {\n          console.log(error);\n        });\n    },\n    refreshProcess() {\n      this.search = \"\";\n      this.filter = \"\";\n      this.showing = false;\n      axios\n        .get(this.api_url + \"user\", {\n          headers: {\n            Authorization: \"Bearer \" + this.userSession.data.token,\n          },\n        })\n        .then((res) => {\n          if (res.data.success) {\n            this.reload();\n            this.userList = res.data.data;\n          }\n        })\n        .catch((error) => {\n          console.log(error);\n        });\n    },\n    goToDetail(id) {\n      this.$router.push({ path: \"/admin/user/\" + id });\n    },\n  },\n  created() {\n    if (sessionStorage.getItem(\"user\") != null) {\n      this.userSession = JSON.parse(sessionStorage.getItem(\"user\"));\n\n      axios\n        .get(this.api_url + \"user\", {\n          headers: {\n            Authorization: \"Bearer \" + this.userSession.data.token,\n          },\n        })\n        .then((res) => {\n          if (res.data.success) {\n            this.reload();\n            this.userList = res.data.data;\n          }\n        })\n        .catch((error) => {\n          console.log(error);\n        });\n    }\n  },\n};\n</script>\n<style scoped>\n.join-date {\n  font-size: 12px;\n}\n\n.card-label {\n  font-size: 14px;\n  margin: 0 !important;\n  line-height: 1.5;\n}\n\n.fade-enter-active {\n  transition: opacity 0.5s ease;\n}\n\n.fade-enter-from {\n  opacity: 0;\n}\n\n.card-list {\n  transition: all 0.5s ease;\n  border: 2px solid rgb(243, 243, 243);\n  cursor: pointer;\n}\n\n.card-list:hover {\n  border: 2px solid rgb(38, 25, 114);\n}\n</style>"],"sourceRoot":""}]}