or: pointer;\n  transition: all 0.4s ease;\n}\n\n.card-allin:hover {\n  box-shadow: 2px 3px 5px#F5A236;\n}\n.card-text {\n  margin-top: -15%;\n}\n\n.card-img-top {\n  width: 80%;\n  -webkit-filter: grayscale(100%); /* Safari 6.0 - 9.0 */\n  filter: grayscale(100%);\n}\n\n.card-allin:hover,\n.card-img-top:hover {\n  -webkit-filter: grayscale(0%); /* Safari 6.0 - 9.0 */\n  filter: grayscale(0%);\n}\n\n.lock {\n  position: absolute;\n  right: 5%;\n}\n\n.lock-module {\n  filter: blur(1px);\n  -webkit-filter: blur(1px);\n}\n</style>"]}]}