,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,EAAE;YAChB,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;UACpB,CAAC,CAAC;MACN;IACF,CAAC;EACH,CAAC;EACD,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE;IACR,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,EAAE,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;;IAE9B,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE;MACb,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;IACvB;EACF,CAAC;AACH,CAAC","file":"C:/Users/dl6/Documents/Project/career-module/src/views/Admin/Module.vue","sourceRoot":"","sourcesContent":["<template>\n  <div id=\"dashboard\">\n    <AdminCheck></AdminCheck>\n    <div class=\"container-fluid ps-0\">\n      <div class=\"row g-0\">\n        <transition name=\"slide\">\n          <div\n            :class=\"sidebar\"\n            class=\"sidebar-admin sticky-top\"\n            v-if=\"sidebarStatus\"\n          >\n            <AdminSidebar :display=\"sidebarToggle\" menu=\"module\"></AdminSidebar>\n          </div>\n        </transition>\n        <div :class=\"header\">\n          <AdminHeader :display=\"sidebarToggle\"></AdminHeader>\n          <div class=\"container p-md-3 p-2\">\n            <!-- Create Module    -->\n            <div class=\"row p-0 mb-2 sticky-top bg-white py-2 mx-0\">\n              <div class=\"col-md-9 px-0\">\n                <router-link to=\"/admin/module/create\">\n                  <button class=\"btn btn-dark mx-0 py-1\">\n                    <vue-feather type=\"plus-circle\" class=\"pe-2\"></vue-feather>\n                    Create a Module\n                  </button>\n                </router-link>\n              </div>\n              <div class=\"col-md-3 mt-md-0 mt-2 px-0\">\n                <input\n                  type=\"text\"\n                  class=\"form-control\"\n                  placeholder=\"Search\"\n                  v-model=\"search.keyword\"\n                  @keypress.enter=\"searchData\"\n                />\n                <transition name=\"fade\">\n                  <vue-feather\n                    type=\"x\"\n                    class=\"float-end pointer\"\n                    stroke=\"red\"\n                    style=\"margin-top: -30px\"\n                    v-if=\"search.keyword\"\n                    @click=\"resetSearch\"\n                  ></vue-feather>\n                </transition>\n              </div>\n            </div>\n\n            <div class=\"col-md-12\" v-if=\"!showing\">\n              <div class=\"text-center my-5\">\n                <vue-feather\n                  type=\"loader\"\n                  size=\"50\"\n                  animation=\"spin\"\n                ></vue-feather>\n              </div>\n            </div>\n\n            <div\n              class=\"col-md-12 text-center\"\n              v-if=\"showing && !moduleList.from\"\n            >\n              <hr />\n              Sorry, module name is not found.\n            </div>\n\n            <!-- Module List  -->\n            <div\n              class=\"row row-cols-md-2 align-items-stretch row-cols-1\"\n              v-if=\"showing\"\n            >\n              <div\n                class=\"col-md-6 mb-2 text-md-start text-center ps-3\"\n                v-if=\"moduleList.from\"\n              >\n                <small\n                  >Showing {{ moduleList.from }} to {{ moduleList.to }} of\n                  {{ moduleList.total }} modules</small\n                >\n              </div>\n              <div\n                class=\"col-md-6 mb-2 text-md-end text-center px-md-0\"\n                v-if=\"moduleList.from\"\n              >\n                <button\n                  class=\"btn border border-1 btn-sm mx-1 px-3\"\n                  :class=\"search.status == null ? 'border-success' : ''\"\n                  @click=\"findByStatus(null)\"\n                >\n                  All\n                </button>\n                <button\n                  class=\"btn border border-1 btn-sm mx-1\"\n                  :class=\"search.status == 0 ? 'border-success' : ''\"\n                  @click=\"findByStatus(0)\"\n                  :disabled=\"status.draft == 0\"\n                >\n                  Draft\n                  <div class=\"d-inline ms-2 text-warning\">\n                    <strong>{{ status.draft }}</strong>\n                  </div>\n                </button>\n                <button\n                  class=\"btn border border-1 btn-sm mx-1\"\n                  :class=\"search.status == 1 ? 'border-success' : ''\"\n                  @click=\"findByStatus(1)\"\n                  :disabled=\"status.publish == 0\"\n                >\n                  Published\n\n                  <div class=\"d-inline ms-2 text-success\">\n                    <strong>{{ status.publish }}</strong>\n                  </div>\n                </button>\n                <button\n                  class=\"btn border border-1 btn-sm mx-1\"\n                  :class=\"search.status == 3 ? 'border-info' : ''\"\n                  @click=\"findByStatus(3)\"\n                  :disabled=\"status.lock == 0\"\n                >\n                  Locked\n                  <div class=\"d-inline ms-2 text-success\">\n                    <strong>{{ status.lock }}</strong>\n                  </div>\n                </button>\n                <button\n                  class=\"btn border border-1 btn-sm mx-1 me-md-3\"\n                  :class=\"search.status == 2 ? 'border-success' : ''\"\n                  @click=\"findByStatus(2)\"\n                  :disabled=\"status.inactive == 0\"\n                >\n                  Inactive\n                  <div class=\"d-inline ms-2 text-danger\">\n                    <strong>{{ status.inactive }}</strong>\n                  </div>\n                </button>\n              </div>\n\n              <div class=\"col mb-3\" v-for=\"i in moduleList.data\" :key=\"i\">\n                <div class=\"card h-100 card-module\" @click=\"moreDetail(i.id)\">\n                  <div class=\"card-body\">\n                    <div class=\"row align-items-center\">\n                      <div class=\"col-md-3\">\n                        <img\n                          v-if=\"i.thumbnail == null\"\n                          src=\"https://dummyimage.com/300x300/e6e6e6/262626.gif&text=Thumbnail\"\n                          class=\"module-picture\"\n                        />\n                        <img\n                          v-if=\"i.thumbnail != null\"\n                          :src=\"'https://api-cm.all-inedu.com/' + i.thumbnail\"\n                          class=\"module-picture\"\n                        />\n                      </div>\n                      <div class=\"col-md-9\">\n                        <h5 class=\"mb-0 mt-md-0 mt-3\">{{ i.module_name }}</h5>\n                        <div class=\"badge badge-info bg-secondary mb-1\">\n                          {{ i.category_name }}\n                        </div>\n                        <div class=\"join-date mb-3\">\n                          <vue-feather\n                            type=\"file-plus\"\n                            size=\"18\"\n                            class=\"me-1\"\n                            stroke=\"green\"\n                          ></vue-feather>\n                          {{ formatDate(i.created_at) }}\n                          <span v-if=\"i.created_at != i.updated_at\">\n                            <vue-feather\n                              class=\"ms-2 me-1\"\n                              type=\"edit\"\n                              size=\"18\"\n                              stroke=\"blue\"\n                            ></vue-feather>\n                            {{ formatDate(i.updated_at) }}\n                          </span>\n                        </div>\n                        <div style=\"height: 100px\">\n                          <p\n                            class=\"mb-3\"\n                            style=\"text-align: justify\"\n                            v-html=\"\n                              i.desc ? formatText(i.desc, 150, '...') : ''\n                            \"\n                          ></p>\n                        </div>\n                        <div class=\"row align-items-center\">\n                          <div class=\"col text-start px-0\">\n                            <button\n                              disabled\n                              class=\"btn py-0 px-3\"\n                              :class=\"\n                                i.status == 0\n                                  ? 'btn-warning'\n                                  : i.status == 1\n                                  ? 'btn-success'\n                                  : i.status == 2\n                                  ? 'btn-danger'\n                                  : 'btn-info'\n                              \"\n                            >\n                              <small>\n                                {{ formatStatus(i.status) }}\n                              </small>\n                            </button>\n                          </div>\n                          <div class=\"col text-end px-0\">\n                            <button\n                              class=\"btn module-btn px-3\"\n                              @click=\"moreDetail(i.id)\"\n                            >\n                              More\n                              <vue-feather\n                                class=\"ms-1\"\n                                type=\"arrow-right-circle\"\n                              ></vue-feather>\n                            </button>\n                          </div>\n                        </div>\n                      </div>\n                    </div>\n                  </div>\n                </div>\n              </div>\n            </div>\n\n            <div class=\"row\">\n              <div\n                class=\"col-md-12 mt-3\"\n                v-if=\"\n                  showing &&\n                  moduleList.data.length > 0 &&\n                  moduleList.last_page > 1\n                \"\n              >\n                <nav aria-label=\"Page navigation example\">\n                  <ul class=\"pagination justify-content-center\">\n                    <li\n                      class=\"page-item\"\n                      v-if=\"moduleList.current_page != 1\"\n                      @click=\"modulePage(moduleList.current_page - 1)\"\n                    >\n                      <a class=\"page-link\">Previous</a>\n                    </li>\n                    <div v-for=\"n in moduleList.last_page\" :key=\"n\">\n                      <li\n                        v-if=\"\n                          moduleList.current_page - 3 < n &&\n                          moduleList.current_page + 3 > n\n                        \"\n                        class=\"page-item\"\n                        :class=\"moduleList.current_page == n ? 'active' : ''\"\n                      >\n                        <a class=\"page-link\" @click=\"modulePage(n)\" href=\"#\">{{\n                          n\n                        }}</a>\n                      </li>\n                    </div>\n                    <li\n                      class=\"page-item\"\n                      v-if=\"moduleList.current_page != moduleList.last_page\"\n                      @click=\"modulePage(moduleList.current_page + 1)\"\n                    >\n                      <a class=\"page-link\">Next</a>\n                    </li>\n                  </ul>\n                </nav>\n              </div>\n\n              <!-- <div class=\"col-md-12 mt-3\">\n                <nav aria-label=\"Page navigation example\">\n                  <ul class=\"pagination justify-content-center\">\n                    <li class=\"page-item disabled\">\n                      <a class=\"page-link\">Previous</a>\n                    </li>\n                    <li class=\"page-item\">\n                      <a class=\"page-link\" href=\"#\">1</a>\n                    </li>\n                    <li class=\"page-item\">\n                      <a class=\"page-link\" href=\"#\">2</a>\n                    </li>\n                    <li class=\"page-item\">\n                      <a class=\"page-link\" href=\"#\">3</a>\n                    </li>\n                    <li class=\"page-item\">\n                      <a class=\"page-link\" href=\"#\">Next</a>\n                    </li>\n                  </ul>\n                </nav>\n              </div> -->\n            </div>\n          </div>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n<script>\nimport AdminCheck from \"@/components/Admin/UserCheck\";\nimport AdminHeader from \"@/components/Admin/Header\";\nimport AdminSidebar from \"@/components/Admin/Sidebar\";\nimport VueFeather from \"vue-feather\";\nimport moment from \"moment\";\nimport Swal from \"sweetalert2\";\n\nexport default {\n  name: \"Student\",\n  components: {\n    AdminCheck,\n    AdminHeader,\n    AdminSidebar,\n    VueFeather,\n  },\n  data() {\n    return {\n      sidebar: \"sidebar-left\",\n      sidebarStatus: true,\n      header: \"content\",\n      user: [],\n      moduleList: [],\n      search: {\n        keyword: \"\",\n        status: null,\n      },\n      status: [],\n      showing: false,\n      moduleStatus: false,\n    };\n  },\n  methods: {\n    sidebarToggle() {\n      if (this.sidebarStatus == true) {\n        this.sidebarStatus = false;\n        this.header = \"full-content\";\n        this.sidebar = \"d-none\";\n      } else {\n        this.sidebarStatus = true;\n        this.sidebar = \"sidebar-left sidebar-mobile\";\n        this.header = \"content\";\n      }\n    },\n    statusProcess(i) {\n      console.log(i);\n      if (this.moduleStatus == true) {\n        Swal.fire({\n          title: \"Are you sure to incative this module?\",\n          showCancelButton: true,\n          confirmButtonText: \"Yes\",\n        }).then((result) => {\n          /* Read more about isConfirmed, isDenied below */\n          if (result.isConfirmed) {\n            Swal.fire(\"Saved!\", \"\", \"success\");\n            this.moduleStatus = false;\n          } else {\n            this.moduleStatus = true;\n          }\n        });\n      } else {\n        Swal.fire({\n          title: \"Are you sure to activate this module?\",\n          showCancelButton: true,\n          confirmButtonText: \"Yes\",\n        }).then((result) => {\n          /* Read more about isConfirmed, isDenied below */\n          if (result.isConfirmed) {\n            Swal.fire(\"Saved!\", \"\", \"success\");\n            this.moduleStatus = true;\n          } else {\n            this.moduleStatus = false;\n          }\n        });\n      }\n    },\n    reload() {\n      this.showing = true;\n    },\n    formatDate(n) {\n      return moment(n).format(\"LL\");\n    },\n    formatText(n, max, suffix) {\n      let regex = /(<([^>]+)>)/gi;\n      let str = n.replace(regex, \"\");\n\n      return str.length < max\n        ? str\n        : `${str.substr(\n            0,\n            str.substr(0, max - suffix.length).lastIndexOf(\" \")\n          )}${suffix}`;\n    },\n    formatStatus(n) {\n      if (n == 0) {\n        return \"Draft\";\n      } else if (n == 1) {\n        return \"Published\";\n      } else if (n == 2) {\n        return \"Inactive\";\n      } else if (n == 3) {\n        return \"Locked\";\n      }\n    },\n    getMouduleData() {\n      this.$axios\n        .get(this.$api_url + \"module\", {\n          headers: {\n            Authorization: \"Bearer \" + this.user.token,\n          },\n        })\n        .then((response) => {\n          this.moduleList = response.data.data.module;\n          this.status = {\n            draft: response.data.data.drafted_module,\n            publish: response.data.data.published_module,\n            inactive: response.data.data.inactive_module,\n            lock: response.data.data.locked_module,\n          };\n          this.showing = true;\n          // console.log(response.data);\n        })\n        .catch((error) => {\n          console.log(error.response);\n        });\n    },\n    searchData() {\n      this.showing = false;\n      this.$axios\n        .post(\n          this.$api_url + \"find/module\",\n          {\n            keyword: this.search.keyword,\n            status: this.search.status,\n          },\n          {\n            headers: {\n              Authorization: \"Bearer \" + this.user.token,\n            },\n          }\n        )\n        .then((response) => {\n          this.showing = true;\n          this.moduleList = response.data.data.module;\n          this.status = {\n            draft: response.data.data.drafted_module,\n            publish: response.data.data.published_module,\n            inactive: response.data.data.inactive_module,\n            lock: response.data.data.locked_module,\n          };\n          // console.log(response.data);\n        })\n        .catch((error) => {\n          this.showing = true;\n          console.log(error.response.data);\n        });\n    },\n    findByStatus(i) {\n      this.search.status = i;\n      this.searchData();\n    },\n    resetSearch() {\n      this.search.keyword = \"\";\n      this.search.status = null;\n      this.searchData();\n    },\n    moreDetail(id) {\n      this.$router.push({ path: \"/admin/module/create/\" + id });\n    },\n    modulePage(n) {\n      this.showing = false;\n      if (this.keyword != \"\" || this.status != \"\") {\n        this.$axios\n          .get(this.$api_url + \"module?page=\" + n, {\n            headers: {\n              Authorization: \"Bearer \" + this.user.token,\n            },\n          })\n          .then((response) => {\n            this.showing = true;\n            this.moduleList = response.data.data.module;\n            this.status = {\n              draft: response.data.data.drafted_module,\n              publish: response.data.data.published_module,\n              inactive: response.data.data.inactive_module,\n              lock: response.data.data.locked_module,\n            };\n          })\n          .catch((error) => {\n            console.log(error);\n          });\n      } else {\n        this.$axios\n          .get(this.$api_url + \"find/module?page=\" + n, {\n            headers: {\n              Authorization: \"Bearer \" + this.user.token,\n            },\n          })\n          .then((response) => {\n            this.showing = true;\n            this.moduleList = response.data.data.module;\n            this.status = {\n              draft: response.data.data.drafted_module,\n              publish: response.data.data.published_module,\n              inactive: response.data.data.inactive_module,\n              lock: response.data.data.locked_module,\n            };\n          })\n          .catch((error) => {\n            console.log(error);\n          });\n      }\n    },\n  },\n  created() {\n    this.user = this.$auth.check();\n\n    if (this.user) {\n      this.getMouduleData();\n    }\n  },\n};\n</script>\n<style scoped>\n.join-date {\n  font-size: 13px;\n}\n\n.fade-enter-active {\n  transition: opacity 0.5s ease;\n}\n\n.fade-enter-from {\n  opacity: 0;\n}\n\n.card-module {\n  border: 3px solid rgba(223, 220, 220, 0.933);\n  transition: all 0.5s ease;\n  cursor: pointer;\n}\n.card-module:hover {\n  border: 3px solid rgb(114, 114, 245);\n}\n\n.module-picture {\n  width: 100%;\n}\n\n.module-btn {\n  border-radius: 20px;\n  background: #e59330;\n  color: #fff;\n  transition: all 0.3s ease;\n  padding: 2px 10px !important;\n}\n\n.module-btn:hover {\n  background: #294593;\n  color: #fff;\n}\n\n.btn {\n  border-radius: 20px;\n}\n\n@media only screen and (max-width: 600px) {\n  .module-picture {\n    width: 100%;\n    height: 150px;\n    object-fit: cover;\n  }\n}\n</style>"]}]}